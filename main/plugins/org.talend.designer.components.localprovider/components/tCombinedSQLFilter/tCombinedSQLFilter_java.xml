<COMPONENT>

	<HEADER
		PLATEFORM="ALL"
		SERIAL=""
		VERSION="0.102"
		STATUS="ALPHA"
		COMPATIBILITY="ALL"
		AUTHOR="Talend"
		RELEASE_DATE="20100112A"
		STARTABLE="false"
		COMBINE="tELTNode"
		PARTITIONING="NONE"
	>
		<SIGNATURE/>
	</HEADER>

	<FAMILIES>
		<FAMILY>ELT/Combined SQL</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL/>
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR NAME="ELTCOMBINE" CTYPE="FLOW" MAX_OUTPUT="1" COLOR="086438" BASE_SCHEMA="FLOW" />
		<!-- <CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="f36300" BASE_SCHEMA="FLOW" /> -->
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>

	<PARAMETERS>
		
		<PARAMETER
			NAME="SCHEMA"
			FIELD="SCHEMA_TYPE"
			NUM_ROW="20"
			CONTEXT="FLOW"
		/>
		
		<PARAMETER 
			NAME="COMBINE_SCHEMA" 
			FIELD="SCHEMA_TYPE" 
			NUM_ROW="25"
			SHOW="false"
			CONTEXT="ELTCOMBINE"
		/>
		
		<PARAMETER 
			NAME="LOGICAL_OP"
			FIELD="CLOSED_LIST"
			NUM_ROW="30"
			REQUIRED="true"
		>
			<ITEMS DEFAULT="AND">
				<ITEM NAME="OR" VALUE="OR" />
				<ITEM NAME="AND" VALUE="AND" />
			</ITEMS>
		</PARAMETER>		

		<PARAMETER
			NAME="CONDITIONS"
			FIELD="TABLE"
			REQUIRED="false"
			NUM_ROW="40"
			NB_LINES="3"
		>
			<ITEMS>
				<ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
				<ITEM NAME="OPERATOR" FIELD="CLOSED_LIST">
					<ITEMS DEFAULT="EQ">
						<ITEM NAME="EQ" VALUE="=" />
						<ITEM NAME="NE" VALUE="&lt;&gt;" />
						<ITEM NAME="GT" VALUE="&gt;" />
						<ITEM NAME="LT" VALUE="&lt;" />
						<ITEM NAME="GE" VALUE="&gt;=" />
						<ITEM NAME="LE" VALUE="&lt;=" />
						<ITEM NAME="LIKE" VALUE="LIKE" />
						<ITEM NAME="IN" VALUE="IN" />
						<ITEM NAME="NOT_IN" VALUE="NOT_IN" />
						<ITEM NAME="EXIST_IN" VALUE="EXIST_IN" />
					</ITEMS>
				</ITEM>
				<ITEM NAME="RVALUE" FIELD="String" />
				<ITEM NAME="NEGATE" FIELD="CHECK" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER
			NAME="USE_ADVANCED"
			FIELD="CHECK"
			REQUIRED="true"
			NUM_ROW="50"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="ADVANCED_COND"
			FIELD="MEMO_SQL"
			NUM_ROW="60"
			NB_LINES="5"
			NOT_SHOW_IF="(USE_ADVANCED=='false')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
	</PARAMETERS>

	<CODEGENERATION>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="FLOW"/>
		<RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW"/>
	</RETURNS>
</COMPONENT>