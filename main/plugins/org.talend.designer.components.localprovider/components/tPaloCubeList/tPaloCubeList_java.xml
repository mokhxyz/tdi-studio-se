<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="2.0"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20091023"
    STARTABLE="true"
    LOG4J_ENABLED="true"
	>
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Business_Intelligence/OLAP_Cube/Palo</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

<CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>




  <PARAMETERS>

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="2"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      REQUIRED="true" FILTER="tPaloConnection"
      NUM_ROW="3"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />

	<PARAMETER
		NAME="SERVER"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="5"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"localhost"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="SERVERPORT"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="6"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"7777"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="USERNAME"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="7"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"admin"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="PASS"
		FIELD="PASSWORD"
		REQUIRED="true"
		NUM_ROW="8"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
	>
	  <DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="DATABASE"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="10"
	>
	  <DEFAULT>"Database"</DEFAULT>
	</PARAMETER>

	 <PARAMETER
      		NAME="SCHEMA"
		FIELD="SCHEMA_TYPE"
		NUM_ROW="30" SHOW="false"
	    >
		<TABLE READONLY="true">
         		<COLUMN KEY="false" NAME="cube_id" NULLABLE="false" TYPE="id_Integer" COMMENT="Id of the cube"/>
         		<COLUMN KEY="false" NAME="cube_name" NULLABLE="false" TYPE="id_String" LENGTH="255" COMMENT="Name of the cube" />
			<COLUMN KEY="false" NAME="cube_dimensions" NULLABLE="false" TYPE="id_Integer" COMMENT="Number of dimensions inside the cube" />
			<COLUMN KEY="false" NAME="cube_cells" NULLABLE="false" TYPE="id_Long" COMMENT="Number of cells(total)" />
			<COLUMN KEY="false" NAME="cube_filled_cells" NULLABLE="false" TYPE="id_Long" COMMENT="Number of filled cells" />
			<COLUMN KEY="false" NAME="cube_status" NULLABLE="false" TYPE="id_Integer" COMMENT="Status of the cube" />
			<COLUMN KEY="false" NAME="cube_type" NULLABLE="false" TYPE="id_Integer" COMMENT="Type of the cube" />
		</TABLE>

	</PARAMETER>


  </PARAMETERS>

  <ADVANCED_PARAMETERS>
	<PARAMETER NAME="DEPLOY_PALO_LIBS" FIELD="CHECK" NUM_ROW="200" SHOW="false">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

  </ADVANCED_PARAMETERS>
    <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="paloIX3" MODULE="paloIX3.jar" MVN="mvn:org.talend.libraries/paloIX3/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.palo/lib/paloIX3.jar" REQUIRED="true" />
      <IMPORT NAME="httpclient-4.2.1" MODULE="httpclient-4.2.1.jar" MVN="mvn:org.talend.libraries/httpclient-4.2.1/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.apache.http/lib/httpclient-4.2.1.jar" REQUIRED="true" />
      <IMPORT NAME="httpmime-4.1.2" MODULE="httpmime-4.1.2.jar" MVN="mvn:org.talend.libraries/httpmime-4.1.2/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.apache.http/lib/httpmime-4.1.2.jar" REQUIRED="true" />
      <IMPORT NAME="httpcore-4.2.1" MODULE="httpcore-4.2.1.jar" MVN="mvn:org.talend.libraries/httpcore-4.2.1/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.apache.http/lib/httpcore-4.2.1.jar" REQUIRED="true" />
      <IMPORT NAME="commons-logging-1.1.1" MODULE="commons-logging-1.1.1.jar" MVN="mvn:org.talend.libraries/commons-logging-1.1.1/6.0.0"  UrlPath="platform:/base/plugins/org.apache.commons.logging_1.1.1.v201101211721.jar" REQUIRED="true" />
      <IMPORT NAME="Talend_CSV" MODULE="talendcsv.jar" MVN="mvn:org.talend.libraries/talendcsv/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.csv/lib/talendcsv.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
    <RETURNS>
	<RETURN NAME="NB_CUBES" TYPE="id_Integer" AVAILABILITY="AFTER" />
    <RETURN NAME="CUBEID" TYPE="id_Integer" AVAILABILITY="FLOW"/>
    <RETURN NAME="CUBENAME" TYPE="id_String" AVAILABILITY="FLOW"/>
  </RETURNS>


</COMPONENT>
