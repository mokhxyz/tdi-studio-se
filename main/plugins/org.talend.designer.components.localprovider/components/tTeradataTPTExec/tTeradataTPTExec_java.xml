  </PARAMETER>

  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
  </CODEGENERATION>

  <RETURNS>
	<RETURN NAME="EXIT_VALUE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
