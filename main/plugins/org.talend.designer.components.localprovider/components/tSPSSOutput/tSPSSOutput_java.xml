<COMPONENT>
	<HEADER PLATEFORM="WINDOWS" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20081030"
		STARTABLE="false" EXTENSION="org.talend.designer.spss"	
		SCHEMA_AUTO_PROPAGATE="true" PARTITIONING="NONE" LOG4J_ENABLED="true"
	>
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>Business_Intelligence/SPSS</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="MAP" FIELD="EXTERNAL" NUM_ROW="1" >
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1" READONLY="false" >
		</PARAMETER>
	      
		<PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="10"
			REQUIRED="true" REPOSITORY_VALUE="FILE_PATH">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.sav"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="WRITE_TYPE" FIELD="CLOSED_LIST" NUM_ROW="20">
			<ITEMS DEFAULT="WRITE">
				<ITEM NAME="APPEND" VALUE="append" />
				<ITEM NAME="WRITE" VALUE="write" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="TRANSLATELABELS" FIELD="CHECK" NUM_ROW="40" SHOW="false"> 
			<DEFAULT>false</DEFAULT> 
		</PARAMETER>


	</PARAMETERS>

      <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="Java_SPSS" MODULE="JSPSS.jar" MVN="mvn:org.talend.libraries/JSPSS/6.0.0"  REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
   </RETURNS>

      
</COMPONENT>
