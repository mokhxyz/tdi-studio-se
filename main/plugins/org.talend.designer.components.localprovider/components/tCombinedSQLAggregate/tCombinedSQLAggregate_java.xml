<COMPONENT>

	<HEADER
		PLATEFORM="ALL"
		SERIAL=""
		VERSION="0.102"
		STATUS="ALPHA"
		COMPATIBILITY="ALL"
		AUTHOR="Talend"
		RELEASE_DATE="20100112A"
		STARTABLE="false"
		COMBINE="tELTNode"
		PARTITIONING="NONE"
	>
		<SIGNATURE/>
	</HEADER>

	<FAMILIES>
		<FAMILY>ELT/Combined SQL</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL/>
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0" />
		<CONNECTOR NAME="ELTCOMBINE" CTYPE="FLOW" MAX_OUTPUT="1" COLOR="086438" BASE_SCHEMA="FLOW" />
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>

	<PARAMETERS>
		
		<PARAMETER
			NAME="SCHEMA"
			FIELD="SCHEMA_TYPE"
			NUM_ROW="20"
			CONTEXT="FLOW"
		/>
		
		<PARAMETER 
			NAME="COMBINE_SCHEMA" 
			FIELD="SCHEMA_TYPE" 
			NUM_ROW="25"
			SHOW="false"
			CONTEXT="ELTCOMBINE"
		/>
		
		<PARAMETER
			NAME="GROUPBYS"
			FIELD="TABLE"
			NUM_ROW="30"
			NB_LINES="4"
		>
			<ITEMS>
				<ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" />
				<ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
			</ITEMS>
		</PARAMETER>
		
		<PARAMETER
		  NAME="OPERATION"
		  FIELD="TABLE"
		  REQUIRED="true"
		  NUM_ROW="40"
		  NB_LINES="4"
		>
		  <ITEMS>
            <ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" CONTEXT="ELTCOMBINE"/>
			<ITEM NAME="FUNCTION" FIELD="CLOSED_LIST">
			  <ITEMS DEFAULT="COUNT">
				<ITEM NAME="COUNT"  VALUE="count"/>
				<ITEM NAME="MIN"    VALUE="min"/>
				<ITEM NAME="MAX"    VALUE="max"/>
				<ITEM NAME="AVG"    VALUE="avg"/>
				<ITEM NAME="SUM"    VALUE="sum"/>
				<ITEM NAME="FIRST"    VALUE="first" />
				<ITEM NAME="LAST"    VALUE="last" />
				<ITEM NAME="DISTINCT"    VALUE="distinct"/>       
				<ITEM NAME="COUNT_DISTINCT"    VALUE="count(distinct @COLUMN)"/>     
			  </ITEMS>
			</ITEM>
			<ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
		  </ITEMS>
		</PARAMETER>
		
	</PARAMETERS>

	<CODEGENERATION>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="FLOW"/>
		<RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW"/>
	</RETURNS>
</COMPONENT>