<COMPONENT>
	<HEADER 
	   PLATEFORM="ALL" 
	   SERIAL="" VERSION="0.101" 
	   STATUS="ALPHA" 
	   COMPATIBILITY="ALL" 
	   AUTHOR="Talend" 
	   RELEASE_DATE="20090320A"	
	   STARTABLE="false" 
	   PARTITIONING="AUTO" 
	   LOG4J_ENABLED="true">
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>Talend MDM</FAMILY>
  </FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="10">
			<DEFAULT />
		</PARAMETER>

	    <PARAMETER
	      NAME="XMLFIELD"
	      FIELD="PREV_COLUMN_LIST"
	      REQUIRED="true"
	      NUM_ROW="15"
	    >
	    </PARAMETER>

		<PARAMETER NAME="MDMURL" FIELD="TEXT" NUM_ROW="20"
			REQUIRED="true">
			<DEFAULT>"http://localhost:8180/talendmdm/services/bulkload"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>"userName"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASSWORD" FIELD="PASSWORD" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>"password"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DATAMODEL" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DATACONCEPT" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
        <PARAMETER NAME="DATACLUSTER" FIELD="TEXT" REQUIRED="true"
            NUM_ROW="45">
            <DEFAULT>""</DEFAULT>
        </PARAMETER>
        <PARAMETER NAME="CONTAINER_TYPE" FIELD="CLOSED_LIST" NUM_ROW="45"
            REQUIRED="true">
            <ITEMS DEFAULT="MASTER">
	            <ITEM NAME="MASTER" VALUE="MASTER" />
	            <ITEM NAME="STAGING" VALUE="STAGING" />
            </ITEMS>
        </PARAMETER>

		<PARAMETER NAME="VALIDATE" FIELD="CHECK" NUM_ROW="50">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="GENERATE_ID" FIELD="CHECK" NUM_ROW="50">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="INSERT_ONLY" FIELD="CHECK" NUM_ROW="50">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MASS_LEVEL" FIELD="TEXT" NUM_ROW="60"
			REQUIRED="true">
			<DEFAULT>500</DEFAULT>
		</PARAMETER>
		
        <PARAMETER NAME="USE_TRANSACTION" FIELD="CHECK" NUM_ROW="70" GROUP="TRANSACTION_GRP">
            <DEFAULT>false</DEFAULT>
        </PARAMETER>
        
        <PARAMETER NAME="TRANSACTION_PROVIDER" FIELD="COMPONENT_LIST" NUM_ROW="80"
            FILTER="tMDMConnection" REQUIRED="true" SHOW_IF="USE_TRANSACTION == 'true'" GROUP="TRANSACTION_GRP">
            <DEFAULT>500</DEFAULT>
        </PARAMETER>
        
	</PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="MDM-Bulkloadclient" MODULE="bulkloadclient.jar" MVN="mvn:org.talend.libraries/bulkloadclient/6.1.0"  REQUIRED="true" />
      <IMPORT NAME="SOAP-codec" MODULE="commons-codec-1.6.jar" MVN="mvn:org.talend.libraries/commons-codec-1.6/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-codec-1.6.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-httpclient" MODULE="commons-httpclient.jar" MVN="mvn:org.talend.libraries/commons-httpclient/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.apache.http/lib/commons-httpclient.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-logging" MODULE="commons-logging.jar" MVN="mvn:org.talend.libraries/commons-logging/6.0.0"  UrlPath="platform:/base/plugins/org.apache.commons.logging_1.1.1.v201101211721.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
  <RETURNS></RETURNS>
</COMPONENT>
