<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070323A"
    STARTABLE="true"
    DATA_AUTO_PROPAGATE="false"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/Teradata</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:TERADATA"
    />

    <PARAMETER
      NAME="EXECUTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="2"
      REQUIRED="true"
    >
      <ITEMS DEFAULT="WINDOWS">
    		<ITEM NAME="WINDOWS" VALUE="Windows" />
    		<ITEM NAME="UNIX" VALUE="Unix" />
    	</ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="4"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="3"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>Teradata</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="DBTABLE"
      NUM_ROW="5"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>   
     
     <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="6"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SCRIPT_PATH"
      FIELD="DIRECTORY"
      NUM_ROW="7"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="8"
      REQUIRED="true"
    >
      <ITEMS DEFAULT="INSERT">
    		<ITEM NAME="INSERT" VALUE="Insert" />
    		<ITEM NAME="UPDATE" VALUE="Update" />
    		<ITEM NAME="UPSERT" VALUE="InsertOrUpdate" />
			<ITEM NAME="DELETE" VALUE="Delete" />
    	</ITEMS>
    </PARAMETER>
    <PARAMETER
      NAME="WHERE"
      FIELD="TEXT"
      NUM_ROW="9"
      REQUIRED="false"
      SHOW_IF="ACTION == 'DELETE'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	<PARAMETER
      NAME="LOAD_FILE"
      FIELD="FILE"
      NUM_ROW="10"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/test.csv"</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="FIELD_SEPARATOR"
      FIELD="TEXT"
      NUM_ROW="12"
      REQUIRED="true"
    >
      <DEFAULT>";"</DEFAULT>
    </PARAMETER>

 	<PARAMETER
      NAME="ERROR_FILE"
      FIELD="FILE"
      NUM_ROW="14"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/error.log"</DEFAULT>
    </PARAMETER>

  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
  	<PARAMETER 
 		    NAME="SPECIFY_LOG_TABLE" 
 		    FIELD="CHECK" 
 		    NUM_ROW="1" 
 		    REQUIRED="true"
 		> 
 		    <DEFAULT>false</DEFAULT> 
 		</PARAMETER>
		<PARAMETER 
 		    NAME="LOG_TABLE_TABLE" 
 		    FIELD="TEXT" 
 		    NUM_ROW="1" 
 		    REQUIRED="true"
			SHOW_IF="SPECIFY_LOG_TABLE=='true'"
 		> 
 		    <DEFAULT>""</DEFAULT> 
 		</PARAMETER>
 		<PARAMETER 
 		    NAME="BEGINLOAD_ADVANCEDPARAM" 
 		    FIELD="TEXT" 
 		    NUM_ROW="2" 
 		    REQUIRED="true" 
 		> 
 		    <DEFAULT>"SESSIONS 8 PACK 600 ARRAYSUPPORT ON CHECKPOINT 60 TENACITY 2 ERRLIMIT 1000"</DEFAULT> 
 		</PARAMETER>
		<PARAMETER 
 		    NAME="RETURN_CODE_DIE" 
 		    FIELD="CHECK" 
 		    NUM_ROW="3" 
 		    REQUIRED="true" 
 		> 
 		    <DEFAULT>false</DEFAULT> 
 		</PARAMETER>
		<PARAMETER 
 		    NAME="RETURN_CODE" 
 		    FIELD="TEXT" 
 		    NUM_ROW="3" 
 		    REQUIRED="true"
			SHOW_IF="RETURN_CODE_DIE=='true'"
 		> 
 		    <DEFAULT>8</DEFAULT> 
 		</PARAMETER>
		<PARAMETER 
 		    NAME="DEFINE_CHARSET" 
 		    FIELD="CHECK" 
 		    NUM_ROW="4" 
 		    REQUIRED="true" 
 		> 
 		    <DEFAULT>false</DEFAULT> 
 		</PARAMETER>
		<PARAMETER 
 		    NAME="CHARSET" 
 		    FIELD="TEXT" 
 		    NUM_ROW="4" 
 		    REQUIRED="true"
			SHOW_IF="DEFINE_CHARSET=='true'"
 		> 
 		    <DEFAULT>"ASCII"</DEFAULT> 
 		</PARAMETER>
  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
  </CODEGENERATION>
   
  <RETURNS>
	<RETURN NAME="EXIT_VALUE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
