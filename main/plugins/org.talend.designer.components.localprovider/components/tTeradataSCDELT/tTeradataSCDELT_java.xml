<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20151030A"
    STARTABLE="true"
    SCHEMA_AUTO_PROPAGATE="false"
    PARTITIONING="NONE"
  >
    <SIGNATURE />
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/Teradata</FAMILY>
    <FAMILY>Business_Intelligence/DB_SCD</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MIN_INPUT="0" MAX_INPUT="1" MAX_OUTPUT="0" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:TERADATA"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="11">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      REQUIRED="true"
      FILTER="tTeradataConnection"
      NUM_ROW="11"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
      DYNAMIC_SETTINGS="true"
     />

    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="20"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>Teradata</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="40"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="40"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE_SRC"
      FIELD="TEXT"
      NUM_ROW="50"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="DBTABLE"
      NUM_ROW="51"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="52"
      NOT_SHOW_IF="PARALLELIZE == 'true'"
    >
      <ITEMS DEFAULT="NONE">
        <ITEM NAME="NONE" VALUE="NONE"/>
        <ITEM NAME="CREATE" VALUE="CREATE"/>
        <ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE"/>
        <ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS"/>
        <ITEM NAME="DROP_IF_EXISTS_AND_CREATE" VALUE="DROP_IF_EXISTS_AND_CREATE" />
        <ITEM NAME="CLEAR" VALUE="CLEAR"/>
        <ITEM NAME="TRUNCATE" VALUE="TRUNCATE"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="CREATE_TABLE_TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="52"
      NOT_SHOW_IF="
                  (PARALLELIZE =='true')
                  OR ((TABLE_ACTION!='CREATE') AND (TABLE_ACTION!='CREATE_IF_NOT_EXISTS') AND (TABLE_ACTION!='DROP_IF_EXISTS_AND_CREATE') AND (TABLE_ACTION!='DROP_CREATE'))"
    >
        <ITEMS DEFAULT="SET">
            <ITEM NAME="SET" VALUE="SET" />
            <ITEM NAME="MULTISET" VALUE="MULTISET" />
        </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="60"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SURROGATE_KEY"
      FIELD="COLUMN_LIST"
      NUM_ROW="61"
      SHOW="true"
    >
      <DEFAULT></DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SK_CREATION"
      FIELD="CLOSED_LIST"
      NUM_ROW="61"
      SHOW="true"
    >
      <ITEMS DEFAULT="AUTO_INCREMENT">
        <ITEM NAME="AUTO_INCREMENT"   VALUE="AUTO_INCREMENT" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="SOURCE_KEYS"
      FIELD="TABLE"
      NUM_ROW="62"
      NB_LINES="2"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="USE_L1"
      FIELD="CHECK"
      NUM_ROW="70"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L1_FIELDS"
      FIELD="TABLE"
      NUM_ROW="71"
      NB_LINES="5"
      SHOW_IF="USE_L1 == 'true'"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="USE_L2"
      FIELD="CHECK"
      NUM_ROW="80"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L2_FIELDS"
      FIELD="TABLE"
      NUM_ROW="81"
      NB_LINES="5"
      SHOW_IF="USE_L2 == 'true'"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="L2_STARTDATE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="82"
      SHOW_IF="USE_L2 == 'true'"
    >
    </PARAMETER>

    <PARAMETER
      NAME="L2_ENDDATE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="82"
      SHOW_IF="USE_L2 == 'true'"
    >
    </PARAMETER>

    <PARAMETER
      NAME="USE_L2_ACTIVE"
      FIELD="CHECK"
      NUM_ROW="83"
      SHOW_IF="USE_L2 == 'true'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L2_ACTIVE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="83"
      SHOW_IF="(USE_L2_ACTIVE == 'true') and (USE_L2 == 'true')"
    >
    </PARAMETER>

    <PARAMETER
      NAME="USE_L2_VERSION"
      FIELD="CHECK"
      NUM_ROW="84"
      SHOW_IF="USE_L2 == 'true'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L2_VERSION_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="84"
      SHOW_IF="(USE_L2_VERSION == 'true') and (USE_L2 == 'true')"
    >
    </PARAMETER>

    <PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="90"
      SHOW="false"
    >
      <DEFAULT>teradata_id</DEFAULT>
    </PARAMETER>
  </PARAMETERS>

  <ADVANCED_PARAMETERS>
    <PARAMETER
        NAME="PROPERTIES"
        FIELD="TEXT"
        NUM_ROW="5"
        REPOSITORY_VALUE="PROPERTIES_STRING"
        SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
        <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="10"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELD_VALUE_INC_NULL"
      FIELD="CHECK"
      NUM_ROW="15"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
        <IMPORT NAME="Driver-Teradata" MODULE="terajdbc4-15.10.00.14.jar" MVN="mvn:org.talend.libraries/terajdbc4-15.10.00.14/6.0.0"   REQUIRED="true" />
        <IMPORT NAME="Driver-Teradata" MODULE="tdgssconfig-15.10.00.14.jar" MVN="mvn:org.talend.libraries/tdgssconfig-15.10.00.14/6.0.0"   REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>

  <RETURNS />
</COMPONENT>
