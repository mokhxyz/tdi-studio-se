<COMPONENT>

     <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070314A"
    STARTABLE="true"
     EXTENSION="org.talend.designer.dbmap"
     SCHEMA_AUTO_PROPAGATE="false"
     DATA_AUTO_PROPAGATE="false"
     HAS_CONDITIONAL_OUTPUTS="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>ELT/Map/Oracle</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
<!--		<CONNECTOR CTYPE="FLOW" BUILTIN="true" MIN_OUTPUT="1" MAX_INPUT="0"/> -->
	    <CONNECTOR CTYPE="TABLE" BUILTIN="true" MIN_INPUT="1" MIN_OUTPUT="1"/>
	    <CONNECTOR CTYPE="TABLE_REF" BUILTIN="true" MIN_INPUT="1" MIN_OUTPUT="1" MAX_OUTPUT="1" />

		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
       
	   <PARAMETER
	     NAME="USE_EXISTING_CONNECTION"
	     FIELD="CHECK"
	     NUM_ROW="2"
	     SHOW_IF="#LINK@CONNECTOR.OUT.TABLE_REF == '0'"
	   >
	     <DEFAULT>false</DEFAULT>
	   </PARAMETER>

	   <PARAMETER
	     NAME="CONNECTION"
	     FIELD="COMPONENT_LIST"
	     REQUIRED="true" FILTER="tOracleConnection"
	     NUM_ROW="2"
	     SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'true')"
	     DYNAMIC_SETTINGS="true"/>

      <PARAMETER NAME="MAP" FIELD="EXTERNAL" NUM_ROW="10">
         <DEFAULT/>
      </PARAMETER>

      <PARAMETER
        NAME="LINK_STYLE"
        FIELD="CLOSED_LIST"
        NUM_ROW="10"
        SHOW_IF="#LINK@CONNECTOR.OUT.TABLE_REF == '0'"
      >
        <ITEMS DEFAULT="">
          <ITEM NAME="AUTO"   VALUE="AUTO" />
          <ITEM NAME="BEZIER_CURVE"   VALUE="BEZIER_CURVE" />
          <ITEM NAME="LINE"   VALUE="LINE" />
        </ITEMS>
      </PARAMETER>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="20"
      SHOW_IF="#LINK@CONNECTOR.OUT.TABLE_REF == '0'"
      REPOSITORY_VALUE="DATABASE:ORACLE"
    />

    <PARAMETER
      NAME="CONNECTION_TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="30"
      REPOSITORY_VALUE="CONNECTION_TYPE"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <ITEMS DEFAULT="ORACLE_SID">
        <ITEM NAME="ORACLE_SID" VALUE="ORACLE_SID" />
        <ITEM NAME="ORACLE_SERVICE_NAME" VALUE="ORACLE_SERVICE_NAME"/>
        <ITEM NAME="ORACLE_OCI" VALUE="ORACLE_OCI" />
		<ITEM NAME="ORACLE_RAC" VALUE="ORACLE_RAC"/>
      </ITEMS>
    </PARAMETER>
	<PARAMETER NAME="DB_VERSION" FIELD="CLOSED_LIST" NUM_ROW="35"
		REPOSITORY_VALUE="DB_VERSION" SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')">
		<ITEMS DEFAULT="ORACLE_11-6">
			<ITEM NAME="ORACLE_12" VALUE="ORACLE_12" />
			<ITEM NAME="ORACLE_11-6" VALUE="ORACLE_11-6" SHOW_IF="false" />
			<ITEM NAME="ORACLE_11" VALUE="ORACLE_11" />
			<ITEM NAME="ORACLE_10" VALUE="ORACLE_10" />
			<ITEM NAME="ORACLE_9" VALUE="ORACLE_9" />
			<ITEM NAME="ORACLE_8" VALUE="ORACLE_8" />
		</ITEMS>
	</PARAMETER>

	<PARAMETER
		NAME="RAC_URL"
		FIELD="TEXT"
		NUM_ROW="37"
		REPOSITORY_VALUE="RAC_URL"
		REQUIRED="true"
		SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE == 'ORACLE_RAC')"
		>
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_RAC')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_RAC')"
    >
      <DEFAULT>"1521"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="50"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_RAC')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="60"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="60"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
      SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="70"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="70"
      SHOW="true"
      READONLY="true"
      SHOW_IF="#LINK@CONNECTOR.OUT.TABLE_REF == '0'"
    >
      <DEFAULT>oracle_id</DEFAULT>
    </PARAMETER>

      <PARAMETER NAME="PREVIEW" FIELD="IMAGE" SHOW="true" NUM_ROW="80">
         <DEFAULT/>
      </PARAMETER>

   </PARAMETERS>

  <ADVANCED_PARAMETERS>
	<PARAMETER
		NAME="PROPERTIES"
		FIELD="TEXT"
		NUM_ROW="1"
		REPOSITORY_VALUE="PROPERTIES_STRING"
		SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="NOTE"
		FIELD="LABEL"
		NUM_ROW="2"
		SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_EXISTING_CONNECTION == 'false')">
		<DEFAULT>*Note: Example for Additional JDBC Parameters: "parameterName1=value1&amp;&amp;parameterName2=value2"</DEFAULT>
	</PARAMETER>

      <PARAMETER
      NAME="USE_HINT_OPTIONS"
      FIELD="CHECK"
      NUM_ROW="10"
      SHOW_IF="#LINK@CONNECTOR.OUT.TABLE_REF == '0'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="HINT_OPTIONS"
      FIELD="TABLE"
      NUM_ROW="10"
      NB_LINES="5"
	  SHOW_IF="(#LINK@CONNECTOR.OUT.TABLE_REF == '0') AND (USE_HINT_OPTIONS == 'true')"
    >
      <ITEMS>
        <ITEM NAME="HINT" VALUE="&quot;/*+ */&quot;" FIELD="TEXT" />
        <ITEM NAME="POSITION" VALUE="AFTER" FIELD="CLOSED_LIST">
        	<ITEMS DEFAULT="AFTER">
        		<ITEM NAME="AFTER"  VALUE="AFTER"/>
        	</ITEMS>
		</ITEM>
        <ITEM NAME="SQL_STMT"  VALUE="true"  FIELD="CLOSED_LIST">
        	<ITEMS DEFAULT="SELECT">
        		<ITEM NAME="SELECT"  VALUE="SELECT"/>
        	</ITEMS>
		</ITEM>
      </ITEMS>
    </PARAMETER>
  </ADVANCED_PARAMETERS>

   <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="ORACLE_12" MODULE="ojdbc7.jar" MVN="mvn:org.talend.libraries/ojdbc7/6.0.0"  REQUIRED_IF="(DB_VERSION == 'ORACLE_12') AND (USE_EXISTING_CONNECTION == 'false')" />
       <IMPORT NAME="ORACLE_11-6" MODULE="ojdbc6.jar" MVN="mvn:org.talend.libraries/ojdbc6/6.0.0"  REQUIRED_IF="(DB_VERSION == 'ORACLE_11-6') AND (USE_EXISTING_CONNECTION == 'false')" />
       <IMPORT NAME="ORACLE_11" MODULE="ojdbc6.jar" MVN="mvn:org.talend.libraries/ojdbc6/6.0.0"  REQUIRED_IF="(DB_VERSION == 'ORACLE_11') AND (USE_EXISTING_CONNECTION == 'false')" />
       <IMPORT NAME="ORACLE_10" MODULE="ojdbc14.jar" MVN="mvn:org.talend.libraries/ojdbc14/6.0.0"  REQUIRED_IF="(DB_VERSION == 'ORACLE_10') AND (USE_EXISTING_CONNECTION == 'false')" />
       <IMPORT NAME="ORACLE_9" MODULE="ojdbc14-9i.jar" MVN="mvn:org.talend.libraries/ojdbc14-9i/6.0.0"  REQUIRED_IF="(DB_VERSION == 'ORACLE_9') AND (USE_EXISTING_CONNECTION == 'false')" />
       <IMPORT NAME="ORACLE_8" MODULE="ojdbc12.jar" MVN="mvn:org.talend.libraries/ojdbc12/6.0.0"  REQUIRED_IF="(DB_VERSION == 'ORACLE_8') AND (USE_EXISTING_CONNECTION == 'false')" />
     </IMPORTS>
   </CODEGENERATION>

   <RETURNS>
   </RETURNS>

   <PLUGINDEPENDENCIES>
   	<PLUGINDEPENDENCY ID="org.talend.designer.abstractmap" />
<!--   	<PLUGINDEPENDENCY ID="org.talend.designer.dbmap" />  -->
   </PLUGINDEPENDENCIES>
</COMPONENT>
