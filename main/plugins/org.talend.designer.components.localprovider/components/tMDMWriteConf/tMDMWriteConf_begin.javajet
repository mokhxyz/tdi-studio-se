                org.talend.mdm.webservice.WSUpdateMetadataItem wsUpdateMetadataItem = new org.talend.mdm.webservice.WSUpdateMetadataItem();
                wsUpdateMetadataItem.setTaskId(taskID);
                wsUpdateMetadataItem.setWsItemPK(wspk);
