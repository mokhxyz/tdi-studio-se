<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070314A"
    STARTABLE="true"
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
    PARTITIONING="AUTO"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/Sybase</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:SYBASE"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="2"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      REQUIRED="true" FILTER="tSybaseConnection"
      NUM_ROW="2"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
      DYNAMIC_SETTINGS="true"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"2648"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="SID"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DB_SCHEMA"
      FIELD="TEXT"
      NUM_ROW="5"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="4"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>SybaseASE</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="6"
      REPOSITORY_VALUE="USERNAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="6"
      REPOSITORY_VALUE="PASSWORD"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="7"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SP_NAME"
      FIELD="TEXT"
      NUM_ROW="8"
      REQUIRED="true"
    >
      <DEFAULT>"myfunction"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="IS_FUNCTION"
      FIELD="CHECK"
      NUM_ROW="9"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="RETURN"
      FIELD="COLUMN_LIST"
      NUM_ROW="9"
      SHOW_IF="IS_FUNCTION == 'true'"
    >
    </PARAMETER>

    <PARAMETER
      NAME="TIMEOUT_INTERVAL"
      FIELD="TEXT"
      NUM_ROW="9"
    >
      <DEFAULT>600</DEFAULT>
    </PARAMETER>

    <PARAMETER
   	  NAME="NOTE"
   	  FIELD="LABEL"
   	  NUM_ROW="9"
    >
   	  <DEFAULT>second(s)</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="SP_ARGS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="10"
      NB_LINES="3"
    >
      <ITEMS>
        <ITEM NAME="COLUMN" FIELD="COLUMN_LIST"/>

        <ITEM NAME="TYPE" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="IN">
            <ITEM NAME="IN" VALUE="IN"/>
            <ITEM NAME="OUT" VALUE="OUT"/>
            <ITEM NAME="RECORDSET" VALUE="RECORDSET"/>
          </ITEMS>
        </ITEM>
      </ITEMS>
    </PARAMETER>
  </PARAMETERS>

  <ADVANCED_PARAMETERS>

  	<PARAMETER
		NAME="PROPERTIES"
		FIELD="TEXT"
		NUM_ROW="3"
		REPOSITORY_VALUE="PROPERTIES_STRING"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

  	<PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="7"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="CONTAIN_MULTI_SELECT"
      FIELD="CHECK"
      NUM_ROW="10"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
       <IMPORT NAME="Driver-Sybase" MODULE="jconn3.jar" MVN="mvn:org.talend.libraries/jconn3/6.0.0"  REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>

  <RETURNS/>
</COMPONENT>
