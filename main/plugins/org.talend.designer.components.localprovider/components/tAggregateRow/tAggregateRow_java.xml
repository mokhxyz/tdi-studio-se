<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE="false"
    VISIBLE="true"
	  PARTITIONING="GROUPBYS.INPUT_COLUMN"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Processing</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MIN_INPUT="1" MAX_INPUT="1" MIN_OUTPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="1"
    >
      <DEFAULT/> 
    </PARAMETER>

    <PARAMETER
      NAME="GROUPBYS"
      FIELD="TABLE"
      NUM_ROW="2"
      NB_LINES="3"
    >
      <ITEMS>
        <ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" />
        <ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="OPERATIONS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="3"
      NB_LINES="5"
    >
      <ITEMS>
        <ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" />
        <ITEM NAME="FUNCTION" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="COUNT">
            <ITEM NAME="COUNT"  VALUE="count"/>
            <ITEM NAME="MIN"    VALUE="min"/>
            <ITEM NAME="MAX"    VALUE="max"/>
            <ITEM NAME="AVG"    VALUE="avg"/>
            <ITEM NAME="SUM"    VALUE="sum"/>
            <ITEM NAME="FIRST"    VALUE="first"/>
            <ITEM NAME="LAST"    VALUE="last"/>
            <ITEM NAME="LIST"    VALUE="list"/>
            <ITEM NAME="LIST_OBJECT"    VALUE="list_object"/>
            <ITEM NAME="DISTINCT"    VALUE="distinct"/>            
            <ITEM NAME="STD_DEV"    VALUE="std_dev"/>
			<ITEM NAME="UNION"    VALUE="union"/>
          </ITEMS>
        </ITEM>
        <ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
        <ITEM NAME="IGNORE_NULL" FIELD="CHECK" />
      </ITEMS>
    </PARAMETER>
    
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
    <PARAMETER
      NAME="LIST_DELIMITER"
      FIELD="TEXT"
      REQUIRED="false"
      NUM_ROW="1"
    >
      <DEFAULT>","</DEFAULT> 
    </PARAMETER>  

	<PARAMETER NAME="USE_FINANCIAL_PRECISION" FIELD="CHECK" REQUIRED="true"
		NUM_ROW="2">
		<DEFAULT>true</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="CHECK_TYPE_OVERFLOW" FIELD="CHECK" REQUIRED="true"
		NUM_ROW="3">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="CHECK_ULP" FIELD="CHECK" REQUIRED="true"
		NUM_ROW="4">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <TEMPLATES INPUT="AGGOUT" OUTPUT="AGGIN">

      <TEMPLATE NAME="AGGOUT" COMPONENT="tAggregateOut">
        <LINK_TO NAME="AGGIN" CTYPE="ROWS_END"/>
      </TEMPLATE>
      
      <TEMPLATE NAME="AGGIN" COMPONENT="tAggregateIn"/>
      
      <TEMPLATE_PARAM SOURCE="self.UNIQUE_NAME" TARGET="AGGOUT.DESTINATION"/>
      <TEMPLATE_PARAM SOURCE="self.UNIQUE_NAME" TARGET="AGGIN.ORIGIN" />
      
      <TEMPLATE_PARAM SOURCE="self.OPERATIONS" TARGET="AGGOUT.OPERATIONS"/>
      <TEMPLATE_PARAM SOURCE="self.OPERATIONS" TARGET="AGGIN.OPERATIONS"/>
      <TEMPLATE_PARAM SOURCE="self.LIST_DELIMITER" TARGET="AGGOUT.LIST_DELIMITER"/>
      <TEMPLATE_PARAM SOURCE="self.GROUPBYS" TARGET="AGGOUT.GROUPBYS"/>
      <TEMPLATE_PARAM SOURCE="self.GROUPBYS" TARGET="AGGIN.GROUPBYS"/>
      <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="AGGOUT.SCHEMA"/>
      <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="AGGIN.SCHEMA"/>
      <TEMPLATE_PARAM SOURCE="self.USE_FINANCIAL_PRECISION" TARGET="AGGOUT.USE_FINANCIAL_PRECISION"/>
      <TEMPLATE_PARAM SOURCE="self.USE_FINANCIAL_PRECISION" TARGET="AGGIN.USE_FINANCIAL_PRECISION" />
      <TEMPLATE_PARAM SOURCE="self.CHECK_TYPE_OVERFLOW" TARGET="AGGOUT.CHECK_TYPE_OVERFLOW"/>
      <TEMPLATE_PARAM SOURCE="self.CHECK_TYPE_OVERFLOW" TARGET="AGGIN.CHECK_TYPE_OVERFLOW" />
      <TEMPLATE_PARAM SOURCE="self.CHECK_ULP" TARGET="AGGOUT.CHECK_ULP"/>
      <TEMPLATE_PARAM SOURCE="self.CHECK_ULP" TARGET="AGGIN.CHECK_ULP" />
      <TEMPLATE_PARAM SOURCE="self.LIST_DELIMITER" TARGET="AGGOUT.LIST_DELIMITER"/>
    </TEMPLATES>
  </CODEGENERATION>
  <RETURNS/>
</COMPONENT>
