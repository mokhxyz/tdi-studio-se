<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="false" PARTITIONING="NONE" LOG4J_ENABLED="true">
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>File/Output</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER
      		NAME="PROPERTY"
      		FIELD="PROPERTY_TYPE"
      		SHOW="true"
      		NUM_ROW="1"
      		REPOSITORY_VALUE="POSITIONAL"
    	/>
   	  	<PARAMETER NAME="USE_EXISTING_DYNAMIC" FIELD="CHECK"
			NUM_ROW="3">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="DYNAMIC" FIELD="COMPONENT_LIST" REQUIRED="true" FILTER="tSetDynamicSchema"
			NUM_ROW="3" SHOW_IF="USE_EXISTING_DYNAMIC == 'true'"/>
		<PARAMETER NAME="USESTREAM" FIELD="CHECK" NUM_ROW="5">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="STREAMNAME" FIELD="TEXT" NUM_ROW="10" SHOW_IF="USESTREAM=='true'">
			<DEFAULT>outputStream</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="10"
			REQUIRED="true" NOT_SHOW_IF="USESTREAM=='true'" REPOSITORY_VALUE="FILE_PATH">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.csv"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="20">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="ROWSEPARATOR" FIELD="TEXT" NUM_ROW="30"
			REPOSITORY_VALUE="ROW_SEPARATOR">
			<DEFAULT>"\n"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="APPEND" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="40" NOT_SHOW_IF="USESTREAM=='true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="INCLUDEHEADER" FIELD="CHECK" NUM_ROW="40" REPOSITORY_VALUE="INCLUDEHEADER">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>		
	    
	    <PARAMETER NAME="COMPRESS" FIELD="CHECK" NUM_ROW="40" SHOW_IF="APPEND=='false'">
	    	<DEFAULT>false</DEFAULT>
	    </PARAMETER>
	    
		<PARAMETER NAME="FORMATS" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="60" NB_LINES="5">
			<ITEMS BASED_ON_SCHEMA="true">

				<ITEM NAME="SIZE" VALUE="10" CONTEXT="LENGTH" />

				<ITEM NAME="PADDING_CHAR" VALUE="' '" />

				<ITEM NAME="ALIGN" FIELD="CLOSED_LIST">
					<ITEMS DEFAULT="LEFT">
						<ITEM NAME="LEFT" VALUE="'L'" />
						<ITEM NAME="CENTER" VALUE="'C'" />
						<ITEM NAME="RIGHT" VALUE="'R'" />
					</ITEMS>
				</ITEM>

				<ITEM NAME="KEEP" FIELD="CLOSED_LIST">
					<ITEMS DEFAULT="ALL">
						<ITEM NAME="ALL" VALUE="'A'" />
						<ITEM NAME="LEFT" VALUE="'L'" />
						<ITEM NAME="MIDDLE" VALUE="'M'" />
						<ITEM NAME="RIGHT" VALUE="'R'" />
					</ITEMS>
				</ITEM>

			</ITEMS>
		</PARAMETER>
	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="ADVANCED_SEPARATOR" FIELD="CHECK" REQUIRED="true" NUM_ROW="31" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="THOUSANDS_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="31"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')">
			<DEFAULT>","</DEFAULT>
		</PARAMETER>		
		
		<PARAMETER NAME="DECIMAL_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="31"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')">
			<DEFAULT>"."</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USE_BYTE" FIELD="CHECK" REQUIRED="true" NUM_ROW="40">
      		<DEFAULT>false</DEFAULT>
    	</PARAMETER>
    	
		<PARAMETER NAME="CREATE" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="50" NOT_SHOW_IF="USESTREAM=='true'">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FLUSHONROW" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="60">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FLUSHONROW_NUM" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="60" SHOW_IF="FLUSHONROW == 'true'">
			<DEFAULT>1</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ROW_MODE" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="65">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="70"
			REQUIRED="true" REPOSITORY_VALUE="ENCODING">
			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>
		
	  	<PARAMETER NAME="DELETE_EMPTYFILE" FIELD="CHECK" REQUIRED="true" NUM_ROW="80" SHOW_IF="(USESTREAM=='false')">
	      <DEFAULT>false</DEFAULT>
	    </PARAMETER>		
	</ADVANCED_PARAMETERS>

	<CODEGENERATION />

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>
