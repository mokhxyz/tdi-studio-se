<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="2.0"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20071212"
    STARTABLE="false"
    DATA_AUTO_PROPAGATE="true"
    PARTITIONING="AUTO"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Business_Intelligence/OLAP_Cube/Palo</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

<CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"  />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="0" />
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE" />
    <CONNECTOR CTYPE="RUN_OK" />
    <CONNECTOR CTYPE="RUN_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>



  <PARAMETERS>
	<PARAMETER
		NAME="SCHEMA"
		FIELD="SCHEMA_TYPE"
		REQUIRED="true"
		NUM_ROW="1"
	/>

	<PARAMETER
		NAME="SERVER"
		FIELD="TEXT"
		REQUIRED="false"
		NUM_ROW="2"
  	>
	  <DEFAULT>"localhost"</DEFAULT>
	</PARAMETER>
	<PARAMETER
		NAME="SERVERPORT"
		FIELD="TEXT"
		REQUIRED="false"
		NUM_ROW="2"
  	>
	  <DEFAULT>"7777"</DEFAULT>
	</PARAMETER>
	<PARAMETER
		NAME="USERNAME"
		FIELD="TEXT"
		REQUIRED="false"
		NUM_ROW="2"
  	>
	  <DEFAULT>"admin"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="USERPASSWORD"
		FIELD="PASSWORD"
		REQUIRED="false"
		NUM_ROW="2"
  	>
	  <DEFAULT>"admin"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="DATABASE"
		FIELD="TEXT"
		REQUIRED="false"
		NUM_ROW="3"
  	>
	  <DEFAULT>"demo"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="CUBE"
		FIELD="TEXT"
		REQUIRED="false"
		NUM_ROW="3"
  	>
	  <DEFAULT>"sales"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="COMMITSIZE"
		FIELD="TEXT"
		REQUIRED="false"
		NUM_ROW="3"
  	>
	  <DEFAULT>100</DEFAULT>
	</PARAMETER>

	<PARAMETER
      		NAME="MEASURE_COLUMN"
      		FIELD="PREV_COLUMN_LIST"
      		REQUIRED="true"
      		NUM_ROW="4"
    	>
    	</PARAMETER>

	<PARAMETER
		NAME="CREATEELEM"
		FIELD="CHECK"
	  	REQUIRED="false"
  		NUM_ROW="5"
  	>
	  <DEFAULT></DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="SAVECUBE"
		FIELD="CHECK"
	  	REQUIRED="false"
  		NUM_ROW="6"
  	>
	  <DEFAULT></DEFAULT>
	</PARAMETER>

  </PARAMETERS>

    <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="paloIX" MODULE="paloIX.jar" MVN="mvn:org.talend.libraries/paloIX/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.palo/lib/paloIX.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>

</COMPONENT>
