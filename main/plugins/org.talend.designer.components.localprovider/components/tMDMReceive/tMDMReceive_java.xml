<COMPONENT>
	<HEADER 
	   PLATEFORM="ALL" 
	   SERIAL="" 
	   VERSION="0.101" 
	   STATUS="ALPHA" 
	   COMPATIBILITY="ALL" 
	   AUTHOR="Talend" 
	   RELEASE_DATE="20090320A" 
	   STARTABLE="true"
	   LOG4J_ENABLED="true" 
	   HAS_CONDITIONAL_OUTPUTS="true" 
	   IS_MULTIPLYING_OUTPUTS="true"
		>
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>Talend MDM</FAMILY>
  </FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    	<CONNECTOR NAME="REJECT" NOT_SHOW_IF="(DIE_ON_ERROR == 'true')" CTYPE="FLOW" MAX_INPUT="1"
			MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
	
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="1" REPOSITORY_VALUE="MDM" />

	   <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="10"/>

		<PARAMETER NAME="SCHEMA_REJECT" FIELD="SCHEMA_TYPE"
			REQUIRED="true" NUM_ROW="11" CONTEXT="REJECT" SHOW="true">
			<TABLE READONLY="true">
				<COLUMN NAME="errorXMLField" TYPE="id_String" LENGTH="255"
					READONLY="false" CUSTOM="true" />
				<COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255"
					READONLY="false" CUSTOM="true" />
				<COLUMN NAME="errorMessage" TYPE="id_String"
					LENGTH="255" READONLY="false" CUSTOM="true" />
			</TABLE>
		</PARAMETER>
		
		  <PARAMETER NAME="CODE" FIELD="MEMO_JAVA" RAW="true" REQUIRED="false" NUM_ROW="2" NB_LINES="9" CONTEXT="begin" SHOW="false">
		    <DEFAULT></DEFAULT>
		  </PARAMETER>
		  
	    <PARAMETER NAME="XMLFIELD" FIELD="COLUMN_LIST" REQUIRED="true" NUM_ROW="15" SHOW="false"/>
	    
		<PARAMETER NAME="USE_XML_FIELD" FIELD="CHECK" NUM_ROW="30" SHOW="false">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="XML_RECORD" FIELD="TEXT" REQUIRED="true" NUM_ROW="30">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<!-- no need to add RAW="true" in XML_PREFIX as the accepted value include quote -->
		<PARAMETER NAME="XML_PREFIX" FIELD="OPENED_LIST" REQUIRED="true" REPOSITORY_VALUE="XPATH_PREFIX" NUM_ROW="35">
			<ITEMS DEFAULT="NONE">
				<ITEM NAME="NONE" VALUE="NONE_ITEM"></ITEM>
				<ITEM NAME="TRIGGER" VALUE="TRIGGER_ITEM"></ITEM>
				<ITEM NAME="PROCESS" VALUE="PROCESS_ITEM"></ITEM>
			</ITEMS>
		</PARAMETER>

	    <PARAMETER NAME="LOOP_QUERY" FIELD="TEXT" REPOSITORY_VALUE="XPATH_QUERY" NUM_ROW="40">
	      <DEFAULT>"/bills/bill/line"</DEFAULT>
	    </PARAMETER>
	
	    <PARAMETER NAME="MAPPING" FIELD="TABLE" REQUIRED="true" REPOSITORY_VALUE="XML_MAPPING" NUM_ROW="50">
	      <ITEMS BASED_ON_SCHEMA="true">
	        <ITEM NAME="QUERY" REPOSITORY_ITEM="XML_QUERY" />
	        <ITEM NAME="NODECHECK" FIELD="CHECK" VALUE="false"/>
	      </ITEMS>
	    </PARAMETER>
	    
	    <PARAMETER NAME="LIMIT" FIELD="TEXT" NUM_ROW="60" REQUIRED="false">
	      <DEFAULT></DEFAULT>
	    </PARAMETER>
	    
		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="70">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
	
	<ADVANCED_PARAMETERS/>

  	<CODEGENERATION>
		<TEMPLATES INPUT="In" OUTPUT="Out">
			<TEMPLATE NAME="In" COMPONENT="tJava">
				<LINK_TO NAME="Out" CTYPE="FLOW" />
			</TEMPLATE>
			
			<TEMPLATE NAME="Out" COMPONENT="tExtractXMLField" />
			
			<TEMPLATE_PARAM SOURCE="self.USE_XML_FIELD" TARGET="Out.USE_XML_FIELD"/>
			<TEMPLATE_PARAM SOURCE="self.XML_RECORD" TARGET="Out.XML_TEXT"/>
			<TEMPLATE_PARAM SOURCE="self.XML_PREFIX" TARGET="Out.XML_PREFIX"/>
						
			<TEMPLATE_PARAM SOURCE="self.CODE" TARGET="In.CODE"/>
			
			<TEMPLATE_PARAM SOURCE="self.XMLFIELD" TARGET="Out.XMLFIELD"/>
			<TEMPLATE_PARAM SOURCE="self.LOOP_QUERY" TARGET="Out.LOOP_QUERY"/>
			<TEMPLATE_PARAM SOURCE="self.MAPPING" TARGET="Out.MAPPING"/>
			<TEMPLATE_PARAM SOURCE="self.LIMIT" TARGET="Out.LIMIT"/>
			<TEMPLATE_PARAM SOURCE="self.MAPPING" TARGET="Out.MAPPING"/>
			<TEMPLATE_PARAM SOURCE="self.DIE_ON_ERROR" TARGET="Out.DIE_ON_ERROR"/>		
				
			<TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="Out.SCHEMA" />
			<TEMPLATE_PARAM SOURCE="self.SCHEMA_REJECT" TARGET="Out.SCHEMA_REJECT" />
		</TEMPLATES>
  </CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>