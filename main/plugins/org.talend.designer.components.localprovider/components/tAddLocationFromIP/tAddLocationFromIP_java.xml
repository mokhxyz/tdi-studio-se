<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    DATA_AUTO_PROPAGATE="false"
	PARTITIONING="AUTO"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Misc</FAMILY>
  </FAMILIES>
  
  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
  
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>
  
  <PARAMETERS>
  
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      READONLY="true" 
      REQUIRED="true"
      NUM_ROW="10"
    >
      <TABLE READONLY="true">
        <COLUMN
          NAME="location"
          TYPE="id_String"
          LENGTH="255"
          READONLY="false"
          CUSTOM="true"
        />
      </TABLE>
    </PARAMETER>

    <PARAMETER
      NAME="WARNING"
      FIELD="LABEL"
      REQUIRED="true"
      NUM_ROW="11"
      COLOR="255;0;0"
    >
      <DEFAULT>The GeoIP.dat database file is available for free, updated monthly on MaxMind website: http://www.maxmind.com/download/geoip/database/</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DATABASE_FILEPATH"
      FIELD="FILE"
      REQUIRED="true"
      NUM_ROW="12"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/GeoIP.dat"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="INPUT_COLUMN"
      GROUP="INPUT"
      FIELD="PREV_COLUMN_LIST"
      REQUIRED="true"
      NUM_ROW="20"
    >
      <DEFAULT></DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="INPUT_HOST_NAME"
      GROUP="INPUT"
      FIELD="RADIO"
      REQUIRED="true"
      NUM_ROW="30"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="INPUT_IP"
      GROUP="INPUT"
      FIELD="RADIO"
      REQUIRED="true"
      NUM_ROW="30"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LOCATION_TYPE_CODE"
      GROUP="LOCATION_TYPE"
      FIELD="RADIO"
      REQUIRED="true"
      NUM_ROW="40"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LOCATION_TYPE_NAME"
      GROUP="LOCATION_TYPE"
      FIELD="RADIO"
      REQUIRED="true"
      NUM_ROW="40"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
  </PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT
        NAME="Geo-IP"
        MODULE="geoip.jar" MVN="mvn:org.talend.libraries/geoip/6.0.0" 
        REQUIRED="true"
      />
    </IMPORTS>
  </CODEGENERATION>
  
  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
  </RETURNS>
</COMPONENT>
