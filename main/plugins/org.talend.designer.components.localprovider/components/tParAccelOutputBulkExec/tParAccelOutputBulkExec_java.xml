<COMPONENT>

	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070320A"
		STARTABLE="true" PARTITIONING="NONE">
		<SIGNATURE />
	</HEADER>

  <FAMILIES>
    <FAMILY>Databases/ParAccel</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>
	
	<PARAMETERS>

		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="1" REPOSITORY_VALUE="DATABASE:PARACCEL" />

		<PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="2" REQUIRED="true"
			REPOSITORY_VALUE="SERVER_NAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

        <PARAMETER
              NAME="TYPE"
              FIELD="TEXT"
              NUM_ROW="3"
              SHOW="false"
              REPOSITORY_VALUE="TYPE"
            >
              <DEFAULT>ParAccel</DEFAULT>
        </PARAMETER>

		<PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="2" REQUIRED="true"
			REPOSITORY_VALUE="PORT">
			<DEFAULT>"5439"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DBNAME" FIELD="TEXT" NUM_ROW="3" REQUIRED="true"
			REPOSITORY_VALUE="SID">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA_DB" FIELD="TEXT" NUM_ROW="3"
			REPOSITORY_VALUE="SCHEMA">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USER" FIELD="TEXT" NUM_ROW="4" REQUIRED="true"
			REPOSITORY_VALUE="USERNAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASS" FIELD="PASSWORD" NUM_ROW="4" REQUIRED="true"
			REPOSITORY_VALUE="PASSWORD">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TABLE" FIELD="DBTABLE" NUM_ROW="5">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TABLE_ACTION" FIELD="CLOSED_LIST"
			NUM_ROW="6" NOT_SHOW_IF="PARALLELIZE == 'true'">
			<ITEMS DEFAULT="NONE">
				<ITEM NAME="NONE" VALUE="NONE" />
				<ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE" />
				<ITEM NAME="CREATE" VALUE="CREATE" />
				<ITEM NAME="CREATE_IF_NOT_EXISTS"
					VALUE="CREATE_IF_NOT_EXISTS" />
				<ITEM NAME="DROP_IF_EXISTS_AND_CREATE" VALUE="DROP_IF_EXISTS_AND_CREATE" />
				<ITEM NAME="CLEAR" VALUE="CLEAR" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="7">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="COPY_MODE" FIELD="CLOSED_LIST" NUM_ROW="10">
			<ITEMS DEFAULT="BASIC">
				<ITEM NAME="BASIC" VALUE="BASIC" />
				<ITEM NAME="PARALLEL" VALUE="PARALLEL" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="15"
			REQUIRED="true">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/bulk.csv"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="MAPPING" FIELD="MAPPING_TYPE" NUM_ROW="20"
			SHOW="false">
			<DEFAULT>postgres_id</DEFAULT>
		</PARAMETER>
	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="FILETYPE" FIELD="CLOSED_LIST" NUM_ROW="1">
			<ITEMS DEFAULT="TXTFILE">
				<ITEM NAME="TXTFILE" VALUE="TXTFILE" />
				<ITEM NAME="GZIP" VALUE="GZIP" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="FIELD_LAYOUT" FIELD="CLOSED_LIST" NUM_ROW="5" SHOW="false">
			<ITEMS DEFAULT="DELIMITED">
				<ITEM NAME="DELIMITED" VALUE="DELIMITED" />
				<ITEM NAME="FIXEDWIDTH" VALUE="FIXEDWIDTH" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="METHOD_FIXED_WIDTH" FIELD="CHECK" NUM_ROW="6" 
			SHOW_IF="(FIELD_LAYOUT=='FIXEDWIDTH')" SHOW="false">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	   	<PARAMETER NAME="ROWSEPARATOR" FIELD="TEXT" NUM_ROW="7" REPOSITORY_VALUE="ROW_SEPARATOR">
	      <DEFAULT>"\n"</DEFAULT>
	    </PARAMETER>
	    <PARAMETER NAME="FIELDS_TERMINATED_BY" FIELD="TEXT" NUM_ROW="7" MAX_LENGTH="1" REPOSITORY_VALUE="FIELD_SEPARATOR">
	      <DEFAULT>";"</DEFAULT>
	    </PARAMETER>
		<PARAMETER NAME="APPEND" FIELD="CHECK" REQUIRED="true" NUM_ROW="8" REPOSITORY_VALUE="APPEND">
		  <DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="FIXED_WIDTH" FIELD="TEXT" NUM_ROW="10" 
			SHOW_IF="(FIELD_LAYOUT=='FIXEDWIDTH' and METHOD_FIXED_WIDTH == 'true')" SHOW="false">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
	  	<PARAMETER NAME="EXPLICIT_IDS" FIELD="CHECK" NUM_ROW="11">
			<DEFAULT>"false"</DEFAULT>
		</PARAMETER>
	  	<PARAMETER NAME="REMOVE_QUOTES" FIELD="CHECK" NUM_ROW="11" 
	  		SHOW_IF="(FIELD_LAYOUT=='DELIMITED')" >
			<DEFAULT>"false"</DEFAULT>
		</PARAMETER>
	  	<PARAMETER NAME="MAX_ERRORS" FIELD="TEXT" NUM_ROW="12">
			<DEFAULT>"2000000000"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="DATE_FORMAT" FIELD="TEXT" NUM_ROW="13">
			<DEFAULT>"YYYY-MM-DD"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TIME_FORMAT" FIELD="TEXT" NUM_ROW="14">
			<DEFAULT>"YYYY-MM-DD HH:MI:SS"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ADDL_OPTIONS" FIELD="TEXT" NUM_ROW="15">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER
			NAME="LOG_FILE"
      		FIELD="FILE"
      		NUM_ROW="20"
      		REQUIRED="true"
    	>
      		<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/paraccel-jdbc.log"</DEFAULT>
    	</PARAMETER>

		<PARAMETER NAME="LOG_LEVEL" FIELD="CLOSED_LIST" NUM_ROW="25">
			<ITEMS DEFAULT="INFO">
				<ITEM NAME="NONE" VALUE="0" />
				<ITEM NAME="INFO" VALUE="1" />
				<ITEM NAME="DEBUG" VALUE="2" />
				<ITEM NAME="PROTOCOL" VALUE="4" />
				<ITEM NAME="INFO_DEBUG" VALUE="3" />
				<ITEM NAME="INFO_PROTOCOL" VALUE="5" />
				<ITEM NAME="DEBUG_PROTOCOL" VALUE="6" />
				<ITEM NAME="ALL" VALUE="7" />
			</ITEMS>
		</PARAMETER>
	<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="30" 
				REQUIRED="true" REPOSITORY_VALUE="ENCODING">
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
    
	</ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <TEMPLATES INPUT="tPOB" OUTPUT="tPBE">

      <TEMPLATE NAME="tPOB" COMPONENT="tParAccelOutputBulk">
        <LINK_TO NAME="tPBE" CTYPE="ROWS_END"/>
      </TEMPLATE>
      
      <TEMPLATE NAME="tPBE" COMPONENT="tParAccelBulkExec"/>
      
      <TEMPLATE_PARAM SOURCE="self.HOST"   TARGET="tPBE.HOST" />
      <TEMPLATE_PARAM SOURCE="self.PORT"   TARGET="tPBE.PORT"   />      
      <TEMPLATE_PARAM SOURCE="self.DBNAME" TARGET="tPBE.DBNAME" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA_DB"   TARGET="tPBE.SCHEMA_DB" />
      <TEMPLATE_PARAM SOURCE="self.USER"   TARGET="tPBE.USER" />
      <TEMPLATE_PARAM SOURCE="self.PASS"   TARGET="tPBE.PASS" />
      <TEMPLATE_PARAM SOURCE="self.TABLE"  TARGET="tPBE.TABLE" />
      <TEMPLATE_PARAM SOURCE="self.TABLE_ACTION"  TARGET="tPBE.TABLE_ACTION" />
      <TEMPLATE_PARAM SOURCE="self.DATA_ACTION"  TARGET="tPBE.DATA_ACTION" />
      <TEMPLATE_PARAM SOURCE="self.OIDS"  TARGET="tPBE.OIDS" />
      <TEMPLATE_PARAM SOURCE="self.HEADER"  TARGET="tPBE.HEADER" />
      <TEMPLATE_PARAM SOURCE="self.TYPE"  TARGET="tPBE.TYPE" />

 	  <TEMPLATE_PARAM SOURCE="self.ENCODING" TARGET="tPOB.ENCODING" />
      <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tPOB.FILENAME" />
      <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tPBE.FILENAME" />
      <TEMPLATE_PARAM SOURCE="self.FILETYPE" TARGET="tPBE.FILETYPE" />
      <TEMPLATE_PARAM SOURCE="self.NULLSTRING" TARGET="tPBE.NULLSTRING" />

      <TEMPLATE_PARAM SOURCE="self.FIELDS_TERMINATED_BY"  TARGET="tPOB.FIELDSEPARATOR" />
      <TEMPLATE_PARAM SOURCE="self.FIELDS_TERMINATED_BY"  TARGET="tPBE.FIELDS_TERMINATED_BY" />
      <TEMPLATE_PARAM SOURCE="self.ESCAPE_CHAR"  TARGET="tPBE.ESCAPE_CHAR" />
      <TEMPLATE_PARAM SOURCE="self.TEXT_ENCLOSURE"  TARGET="tPBE.TEXT_ENCLOSURE" />
      <TEMPLATE_PARAM SOURCE="self.FORCENOTNULL"  TARGET="tPBE.FORCENOTNULL" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA"  TARGET="tPBE.SCHEMA" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA"  TARGET="tPOB.SCHEMA" />
      <TEMPLATE_PARAM SOURCE="self.MAPPING"  TARGET="tPBE.MAPPING" />
      <TEMPLATE_PARAM SOURCE="self.LOG_FILE"  TARGET="tPBE.LOG_FILE" />
      <TEMPLATE_PARAM SOURCE="self.LOG_LEVEL"  TARGET="tPBE.LOG_LEVEL" />
      <TEMPLATE_PARAM SOURCE="self.FILETYPE"  TARGET="tPBE.FILETYPE" />
      <TEMPLATE_PARAM SOURCE="self.FIELD_LAYOUT"  TARGET="tPBE.FIELD_LAYOUT" />
      <TEMPLATE_PARAM SOURCE="self.METHOD_FIXED_WIDTH"  TARGET="tPBE.METHOD_FIXED_WIDTH" />
      <TEMPLATE_PARAM SOURCE="self.ROWSEPARATOR"  TARGET="tPOB.ROWSEPARATOR" />
      <TEMPLATE_PARAM SOURCE="self.APPEND"  TARGET="tPOB.APPEND" />

      <TEMPLATE_PARAM SOURCE="self.FIXED_WIDTH"  TARGET="tPBE.FIXED_WIDTH" />
      <TEMPLATE_PARAM SOURCE="self.EXPLICIT_IDS"  TARGET="tPBE.EXPLICIT_IDS" />
      <TEMPLATE_PARAM SOURCE="self.REMOVE_QUOTES"  TARGET="tPBE.REMOVE_QUOTES" />
      <TEMPLATE_PARAM SOURCE="self.MAX_ERRORS"  TARGET="tPBE.MAX_ERRORS" />
      <TEMPLATE_PARAM SOURCE="self.DATE_FORMAT"  TARGET="tPBE.DATE_FORMAT" />
      <TEMPLATE_PARAM SOURCE="self.TIME_FORMAT"  TARGET="tPBE.TIME_FORMAT" />
      <TEMPLATE_PARAM SOURCE="self.ADDL_OPTIONS"  TARGET="tPBE.ADDL_OPTIONS" />

    </TEMPLATES>
  </CODEGENERATION>
	<RETURNS />
</COMPONENT>
