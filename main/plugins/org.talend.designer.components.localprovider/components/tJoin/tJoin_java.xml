<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
    HAS_CONDITIONAL_OUTPUTS="true"
    VISIBLE="true"
    PARTITIONING="NONE"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Processing</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MIN_INPUT="2" MAX_INPUT="2" MIN_OUTPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="LOOKUP" COMPONENT="tHash"/>    
    <CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" COLOR="FF0000" BASE_SCHEMA="FLOW" />    
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="1"
    >
      <DEFAULT/> 
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA_REJECT"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="2"
      CONTEXT="REJECT"
      SHOW="true">
    </PARAMETER>
    
    <PARAMETER
      NAME="USE_LOOKUP_COLS"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="10"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LOOKUP_COLS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="11"
      NB_LINES="3"
      SHOW_IF="(USE_LOOKUP_COLS=='true')"
    >
      <ITEMS>
        <ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" />
        <ITEM NAME="LOOKUP_COLUMN" FIELD="LOOKUP_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="JOIN_KEY"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="21"
      NB_LINES="3"
    >
      <ITEMS>
        <ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
        <ITEM NAME="LOOKUP_COLUMN" FIELD="LOOKUP_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="USE_INNER_JOIN"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="30"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION/>

  <RETURNS/>
</COMPONENT>
