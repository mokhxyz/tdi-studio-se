<!-- $Id: tWriteXMLField.xml 1549 2007-01-23 04:34:23 +0000 (星期二, 23 一月 2007) xzhang $ -->
<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="false"  EXTENSION="org.talend.designer.fileoutputxml" 
		    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
	PARTITIONING="NONE"
	>
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>XML</FAMILY>
    <FAMILY>Processing/Fields</FAMILY>
  </FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1" MIN_OUTPUT="1" MIN_INPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER
	      NAME="XMLFIELD"
	      FIELD="COLUMN_LIST"
	      REQUIRED="true"
	      NUM_ROW="1"
	    >
    	</PARAMETER>
		
		<PARAMETER NAME="ROOT" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="10" NB_LINES="3" SHOW="false">
			<ITEMS>
				<ITEM NAME="PATH" FIELD="TEXT" />
				<ITEM NAME="COLUMN" FIELD="TEXT" />
				<ITEM NAME="VALUE" FIELD="TEXT" />
				<ITEM NAME="ATTRIBUTE" FIELD="TEXT" />
				<ITEM NAME="ORDER" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="GROUP" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="20" NB_LINES="5" SHOW="false">
			<ITEMS>
				<ITEM NAME="PATH" FIELD="TEXT" />
				<ITEM NAME="COLUMN" FIELD="TEXT" />
				<ITEM NAME="VALUE" FIELD="TEXT" />
				<ITEM NAME="ATTRIBUTE" FIELD="TEXT" />
				<ITEM NAME="ORDER" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="LOOP" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="30" SHOW="false">
			<ITEMS>
				<ITEM NAME="PATH" FIELD="TEXT" />
				<ITEM NAME="COLUMN" FIELD="TEXT" />
				<ITEM NAME="VALUE" FIELD="TEXT" />
				<ITEM NAME="ATTRIBUTE" FIELD="TEXT" />
				<ITEM NAME="ORDER" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="MAP" FIELD="EXTERNAL" NUM_ROW="40">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="50">
			<DEFAULT />
		</PARAMETER>

	    <PARAMETER
	      NAME="GROUPBYS"
	      FIELD="TABLE"
	      NUM_ROW="60"
	      NB_LINES="3"
	    >
	      <ITEMS>
	        <ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
	      </ITEMS>
	    </PARAMETER>		

	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="REMOVE_HEADER" FIELD="CHECK" NUM_ROW="5">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="CREATE" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="10" SHOW="false">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CREATE_EMPTY_ELEMENT" FIELD="CHECK"
			REQUIRED="false" NUM_ROW="11">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="EXPAND_EMPTY_ELM" FIELD="CHECK" NUM_ROW="11"
			SHOW_IF="CREATE_EMPTY_ELEMENT=='true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="OUTPUT_AS_XSD" FIELD="CHECK"
			REQUIRED="false" NUM_ROW="15">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="XSD_FILE" FIELD="FILE"
			REQUIRED="false" NUM_ROW="16" SHOW_IF="OUTPUT_AS_XSD == 'true')">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/file.xsd"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ADVANCED_SEPARATOR" FIELD="CHECK" REQUIRED="true" NUM_ROW="31" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="THOUSANDS_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="32"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')">
			<DEFAULT>","</DEFAULT>
		</PARAMETER>		
		
		<PARAMETER NAME="DECIMAL_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="32"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')">
			<DEFAULT>"."</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="GENERATION_MODE" FIELD="CLOSED_LIST" NUM_ROW="40">
	      <ITEMS DEFAULT="DOM4J">
	        <ITEM NAME="DOM4J" VALUE="Dom4j" />
	        <ITEM NAME="NULL" VALUE="Null"/>
	      </ITEMS>
	    </PARAMETER>   
		
		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="50"
			REQUIRED="true">
			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>	
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<TEMPLATES INPUT="Out" OUTPUT="In">
			<TEMPLATE NAME="Out" COMPONENT="tWriteXMLFieldOut">
				<LINK_TO NAME="In" CTYPE="COMPONENT_OK" />
			</TEMPLATE>
			
			<TEMPLATE NAME="In" COMPONENT="tWriteXMLFieldIn" />
			
			<TEMPLATE_PARAM SOURCE="self.GROUPBYS" TARGET="Out.GROUPBYS"/>
			
			<TEMPLATE_PARAM SOURCE="self.XMLFIELD"
				TARGET="In.XMLFIELD" />
			<TEMPLATE_PARAM SOURCE="self.ROOT"
				TARGET="Out.ROOT" />
			<TEMPLATE_PARAM SOURCE="self.GROUP"
				TARGET="Out.GROUP" />
			<TEMPLATE_PARAM SOURCE="self.LOOP"
				TARGET="Out.LOOP" />
			<TEMPLATE_PARAM SOURCE="self.REMOVE_HEADER"
				TARGET="Out.REMOVE_HEADER" />
			<TEMPLATE_PARAM SOURCE="self.CREATE"
				TARGET="Out.CREATE" />
			<TEMPLATE_PARAM SOURCE="self.CREATE_EMPTY_ELEMENT"
				TARGET="Out.CREATE_EMPTY_ELEMENT" />
			<TEMPLATE_PARAM SOURCE="self.EXPAND_EMPTY_ELM"
				TARGET="Out.EXPAND_EMPTY_ELM" />
			<TEMPLATE_PARAM SOURCE="self.OUTPUT_AS_XSD"
				TARGET="Out.OUTPUT_AS_XSD" />
			<TEMPLATE_PARAM SOURCE="self.XSD_FILE"
				TARGET="Out.XSD_FILE" />
			<TEMPLATE_PARAM SOURCE="self.ADVANCED_SEPARATOR"
				TARGET="Out.ADVANCED_SEPARATOR" />
			<TEMPLATE_PARAM SOURCE="self.THOUSANDS_SEPARATOR"
				TARGET="Out.THOUSANDS_SEPARATOR" />
			<TEMPLATE_PARAM SOURCE="self.DECIMAL_SEPARATOR"
				TARGET="Out.DECIMAL_SEPARATOR" />
			<TEMPLATE_PARAM SOURCE="self.GENERATION_MODE"
				TARGET="Out.GENERATION_MODE" />
			<TEMPLATE_PARAM SOURCE="self.ENCODING"
				TARGET="Out.ENCODING" />
				
			<TEMPLATE_PARAM SOURCE="self.UNIQUE_NAME" TARGET="In.DESTINATION" />
			
			<TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="In.SCHEMA" />
			<TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="Out.SCHEMA" />
		</TEMPLATES>
	</CODEGENERATION>

		
	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>