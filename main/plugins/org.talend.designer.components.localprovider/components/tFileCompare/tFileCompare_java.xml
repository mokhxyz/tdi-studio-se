<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A" LOG4J_ENABLED="true"
		STARTABLE="true" SCHEMA_AUTO_PROPAGATE="false" PARTITIONING="AUTO">
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>File/Management</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" READONLY="true" REQUIRED="true"
			NUM_ROW="1" >
			<TABLE READONLY="true">
				<COLUMN NAME="file" TYPE="id_String" LENGTH="255" />
				<COLUMN NAME="file_ref" TYPE="id_String" LENGTH="255" />
				<COLUMN NAME="moment" TYPE="id_Date"
					PATTERN='"yyyy-MM-dd HH:mm:ss"' />
				<COLUMN NAME="job" TYPE="id_String" LENGTH="50" />
				<COLUMN NAME="component" TYPE="id_String" LENGTH="255" />
				<COLUMN NAME="differ" TYPE="id_Integer" LENGTH="1" />
				<COLUMN NAME="message" TYPE="id_String" LENGTH="255" />
			</TABLE>
		</PARAMETER>

		<PARAMETER NAME="COMPARISON_MODE" FIELD="CLOSED_LIST"
			NUM_ROW="2">
			<ITEMS DEFAULT="TEXT_CMP">
				<ITEM NAME="TEXT_CMP" VALUE="TEXT_CMP" />
				<ITEM NAME="BINARY_CMP" VALUE="BINARY_CMP" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="FILE" FIELD="FILE" REQUIRED="true"
			NUM_ROW="10">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/need_test.txt"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FILE_REF" FIELD="FILE" REQUIRED="true"
			NUM_ROW="20">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/reference.txt"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DIFFER_MESSAGE" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>"[job " + jobName + "] Files differ"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="NO_DIFFER_MESSAGE" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40">
			<DEFAULT>"[job " + jobName + "] Files are identical"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PRINT" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="50">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="45"
			REQUIRED="true" REPOSITORY_VALUE="ENCODING">
			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION />

	<RETURNS>
		<RETURN NAME="DIFFERENCE" TYPE="id_Boolean" AVAILABILITY="FLOW" />
	</RETURNS>

</COMPONENT>
