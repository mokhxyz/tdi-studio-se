<COMPONENT>
<HEADER
	PLATEFORM="ALL"
	SERIAL=""
	VERSION="0.102"
	STATUS="ALPHA"
	COMPATIBILITY="ALL"
	AUTHOR="stefeclipse.free.fr/blog"
	RELEASE_DATE="20070611A"
	STARTABLE="true"
	PARTITIONING="NONE"
	>
	<SIGNATURE/>
</HEADER>

<FAMILIES>
	<FAMILY>Business_Intelligence/Charts</FAMILY>
</FAMILIES>

<DOCUMENTATION>
	<URL/>
</DOCUMENTATION>

<CONNECTORS>
	<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
	<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
	<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
	<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="0" />
	<CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="0"/>
	<CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="0"/>
	<CONNECTOR CTYPE="RUN_IF" MAX_INPUT="0"/>
</CONNECTORS>

<PARAMETERS>

<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" READONLY="true" REQUIRED="true" NUM_ROW="1">
	<TABLE  READONLY="true">
		<COLUMN NAME="series" TYPE="id_String" LENGTH="50"/>
		<COLUMN NAME="x" TYPE="id_Integer"/>
		<COLUMN NAME="y" TYPE="id_Integer"/>
	</TABLE>
</PARAMETER>

<PARAMETER NAME="GENERATED_IMAGE_PATH" FIELD="FILE" NUM_ROW="20" REQUIRED="true">
	<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.png"</DEFAULT>
</PARAMETER>

<PARAMETER NAME="CHART_TITLE" FIELD="STRING" NUM_ROW="30" REQUIRED="true">
	<DEFAULT>"Chart title"</DEFAULT>
</PARAMETER>

<PARAMETER NAME="DOMAIN_AXIS_LABEL" FIELD="String" NUM_ROW="35" REQUIRED="true">
	<DEFAULT>"Domain axis label"</DEFAULT>
</PARAMETER>

<PARAMETER NAME="RANGE_AXIS_LABEL" FIELD="String" NUM_ROW="35" REQUIRED="true">
	<DEFAULT>"Range axis label"</DEFAULT>
</PARAMETER>

<PARAMETER NAME="PLOT_ORIENTATION" FIELD="CLOSED_LIST" NUM_ROW="40" REQUIRED="true">
	<ITEMS DEFAULT="VERTICAL">
		<ITEM NAME="VERTICAL" VALUE="org.jfree.chart.plot.PlotOrientation.VERTICAL"/>
		<ITEM NAME="HORIZONTAL" VALUE="org.jfree.chart.plot.PlotOrientation.HORIZONTAL"/>
	</ITEMS>
</PARAMETER>

<PARAMETER NAME="INCLUDE_LEGEND" FIELD="CHECK" NUM_ROW="40" REQUIRED="true">
	<DEFAULT>true</DEFAULT>
</PARAMETER>

<PARAMETER NAME="IMAGE_WIDTH" FIELD="INTEGER" NUM_ROW="50" REQUIRED="true">
	<DEFAULT>500</DEFAULT>
</PARAMETER>

<PARAMETER NAME="IMAGE_HEIGHT" FIELD="INTEGER" NUM_ROW="50" REQUIRED="true">
	<DEFAULT>500</DEFAULT>
</PARAMETER>

<PARAMETER NAME="MOVING_AVERAGE" FIELD="CHECK" NUM_ROW="60">
	<DEFAULT>false</DEFAULT>
</PARAMETER>

<PARAMETER NAME="MOVING_AVERAGE_PERIOD" FIELD="TEXT" NUM_ROW="60" SHOW_IF="MOVING_AVERAGE == 'true'">
	<DEFAULT>20</DEFAULT>
</PARAMETER>

<PARAMETER NAME="LOWER_BOUND" FIELD="TEXT" NUM_ROW="70">
	<DEFAULT></DEFAULT>
</PARAMETER>

<PARAMETER NAME="UPPER_BOUND" FIELD="TEXT" NUM_ROW="70">
	<DEFAULT></DEFAULT>
</PARAMETER>

<PARAMETER NAME="BACKGROUND_CHART" FIELD="CLOSED_LIST" NUM_ROW="80">
	<ITEMS DEFAULT="WHITE">
		<ITEM NAME="WHITE" VALUE="java.awt.Color.WHITE"/>
		<ITEM NAME="LIGHT_GRAY" VALUE="java.awt.Color.LIGHT_GRAY"/>
		<ITEM NAME="GRAY" VALUE="java.awt.Color.GRAY"/>
		<ITEM NAME="DARK_GRAY" VALUE="java.awt.Color.DARK_GRAY"/>
		<ITEM NAME="BLACK" VALUE="java.awt.Color.BLACK"/>
		<ITEM NAME="RED" VALUE="java.awt.Color.RED"/>
		<ITEM NAME="PINK" VALUE="java.awt.Color.PINK"/>
		<ITEM NAME="ORANGE" VALUE="java.awt.Color.ORANGE"/>
		<ITEM NAME="YELLOW" VALUE="java.awt.Color.YELLOW"/>
		<ITEM NAME="GREEN" VALUE="java.awt.Color.GREEN"/>
		<ITEM NAME="MAGENTA" VALUE="java.awt.Color.MAGENTA"/>
		<ITEM NAME="CYAN" VALUE="java.awt.Color.CYAN"/>
		<ITEM NAME="BLUE" VALUE="java.awt.Color.BLUE"/>
	</ITEMS>
</PARAMETER>

<PARAMETER NAME="BACKGROUND_PLOT" FIELD="CLOSED_LIST" NUM_ROW="80">
	<ITEMS DEFAULT="WHITE">
		<ITEM NAME="WHITE" VALUE="java.awt.Color.WHITE"/>
		<ITEM NAME="LIGHT_GRAY" VALUE="java.awt.Color.LIGHT_GRAY"/>
		<ITEM NAME="GRAY" VALUE="java.awt.Color.GRAY"/>
		<ITEM NAME="DARK_GRAY" VALUE="java.awt.Color.DARK_GRAY"/>
		<ITEM NAME="BLACK" VALUE="java.awt.Color.BLACK"/>
		<ITEM NAME="RED" VALUE="java.awt.Color.RED"/>
		<ITEM NAME="PINK" VALUE="java.awt.Color.PINK"/>
		<ITEM NAME="ORANGE" VALUE="java.awt.Color.ORANGE"/>
		<ITEM NAME="YELLOW" VALUE="java.awt.Color.YELLOW"/>
		<ITEM NAME="GREEN" VALUE="java.awt.Color.GREEN"/>
		<ITEM NAME="MAGENTA" VALUE="java.awt.Color.MAGENTA"/>
		<ITEM NAME="CYAN" VALUE="java.awt.Color.CYAN"/>
		<ITEM NAME="BLUE" VALUE="java.awt.Color.BLUE"/>
	</ITEMS>
</PARAMETER>

</PARAMETERS>

<CODEGENERATION>
	<IMPORTS>
		<IMPORT NAME="JFreeChart" MODULE="jfreechart-1.0.3.jar" MVN="mvn:org.talend.libraries/jfreechart-1.0.3/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.jfreechart/lib/jfreechart-1.0.3.jar" REQUIRED="true"/>
		<IMPORT NAME="JCommon" MODULE="jcommon-1.0.6.jar" MVN="mvn:org.talend.libraries/jcommon-1.0.6/6.0.0"  REQUIRED="true"/>
	</IMPORTS>
</CODEGENERATION>

<RETURNS>
	<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
</RETURNS>

</COMPONENT>
