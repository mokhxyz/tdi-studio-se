<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
    IS_MULTIPLYING_OUTPUTS="true"
    PARTITIONING="AUTO"
    LOG4J_ENABLED="true"
  ><!---->
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

	<PARAMETER
      NAME="RECORDSET_FIELD"
      FIELD="PREV_COLUMN_LIST"
      REQUIRED="true"
      NUM_ROW="1"
    >
    </PARAMETER>
    
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="3"
    >
      <!--SHOW_IF="PROPERTY_TYPE=='BUILT_IN'"-->
      <DEFAULT/>
    </PARAMETER>

	<PARAMETER
      NAME="ATTRIBUTE_TABLE"
      FIELD="TABLE"
      REQUIRED="true"
      NUM_ROW="5"
      NB_LINES="5"
    >
      <ITEMS BASED_ON_SCHEMA="true">
        <ITEM NAME="VALUE"/>
      </ITEMS>
    </PARAMETER>
  </PARAMETERS>

  <CODEGENERATION>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
