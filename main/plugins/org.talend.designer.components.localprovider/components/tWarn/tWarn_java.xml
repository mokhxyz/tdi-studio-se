<!-- $Id: tWarn_perl.xml 1528 2007-01-19 17:03:36Z smallet $ -->
<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
       SERIAL=""
      VERSION="0.101"
       STATUS="ALPHA"
         
COMPATIBILITY="ALL"
       AUTHOR="Talend"
 RELEASE_DATE="20050320A"
    STARTABLE="true"
    PARTITIONING="AUTO"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Logs_Errors</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="MESSAGE" FIELD="TEXT" REQUIRED="false" NUM_ROW="1">
      <DEFAULT>"this is a warning"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="CODE" FIELD="TEXT" REQUIRED="false" NUM_ROW="2">
      <DEFAULT>42</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PRIORITY"
      FIELD="CLOSED_LIST"
      NUM_ROW="30"
    >
      <ITEMS DEFAULT="WARNING">
        <ITEM NAME="TRACE"   VALUE="1" />
        <ITEM NAME="DEBUG"   VALUE="2" />
        <ITEM NAME="INFO"    VALUE="3" />
        <ITEM NAME="WARNING" VALUE="4" />
        <ITEM NAME="ERROR"   VALUE="5" />
        <ITEM NAME="FATAL"   VALUE="6" />
      </ITEMS>
    </PARAMETER>
   
   </PARAMETERS>

   <CODEGENERATION/>

   <RETURNS>   
       <RETURN NAME="WARN_MESSAGES" TYPE="id_String" AVAILABILITY="AFTER"/>  
       <RETURN NAME="WARN_CODE" TYPE="id_Integer" AVAILABILITY="AFTER"/>  
       <RETURN NAME="WARN_PRIORITY" TYPE="id_Integer" AVAILABILITY="AFTER"/>
   </RETURNS>  

</COMPONENT>
