<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
	PARTITIONING="NONE"
  >
    <SIGNATURE />
  </HEADER>

  <FAMILIES>
    <FAMILY>Internet</FAMILY>
  </FAMILIES>
  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
   <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>
  <PARAMETERS>
    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="1"
      REQUIRED="true"
      REPOSITORY_VALUE="FILE_PATH"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.xml"</DEFAULT>
    </PARAMETER>
      <PARAMETER
      NAME="APPEND"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="2"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

     <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="2"
      REQUIRED="true"
      REPOSITORY_VALUE="ENCODING"
    >
      <DEFAULT>"UTF-8"</DEFAULT>
    </PARAMETER>

	<PARAMETER NAME="RSS" FIELD="RADIO" REQUIRED="true"
		NUM_ROW="3" GROUP="MODE">
		<DEFAULT>true</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="ATOM" FIELD="RADIO" REQUIRED="true"
		NUM_ROW="3" GROUP="MODE">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

    <PARAMETER
    NAME="TITLE"
    FIELD="TEXT"
    REQUIRED="true"
    GROUP="CHANNEL"
    NUM_ROW="11"
    SHOW_IF="RSS=='true'">
    <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
    NAME="DESCRIPTION"
    FIELD="TEXT"
    REQUIRED="true"
    GROUP="CHANNEL"
    NUM_ROW="12"
    SHOW_IF="RSS=='true'">
    <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
    NAME="PUBDATE"
    FIELD="TEXT"
    REQUIRED="true"
    GROUP="CHANNEL"
    NUM_ROW="13"
    SHOW_IF="RSS=='true'">
    <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
    NAME="LINK"
    FIELD="TEXT"
    REQUIRED="true"
    GROUP="CHANNEL"
    NUM_ROW="14"
    SHOW_IF="RSS=='true'">
    <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
    NAME="aTITLE"
    FIELD="TEXT"
    REQUIRED="true"
    GROUP="FEED"
    NUM_ROW="11"
    SHOW_IF="ATOM=='true'">
    <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
    NAME="aLINK"
    FIELD="TEXT"
    REQUIRED="true"
    GROUP="FEED"
    NUM_ROW="12"
    SHOW_IF="ATOM=='true'">
    <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
    NAME="aID"
    FIELD="TEXT"
    REQUIRED="true"
    GROUP="FEED"
    NUM_ROW="13"
    SHOW_IF="ATOM=='true'">
    <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
    NAME="aUPDATED"
    FIELD="TEXT"
    REQUIRED="true"
    GROUP="FEED"
    NUM_ROW="14"
    SHOW_IF="ATOM=='true'">
    <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
    NAME="aAUTHOR"
    FIELD="TEXT"
    REQUIRED="true"
    GROUP="FEED"
    NUM_ROW="15"
    SHOW_IF="ATOM=='true'">
    <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ELEMENTS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="20"
      NB_LINES="8"
    >
      <ITEMS>
        <ITEM NAME="ELEMENT_NAME" FIELD="String" />
        <ITEM NAME="ELEMENT_VALUE" FIELD="String"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="30">
      	<TABLE IF="ATOM=='true'">
      		<COLUMN KEY="false" LENGTH="50" NAME="id" TYPE="id_String" NULLABLE="true" READONLY="true"/>
      		<COLUMN KEY="false" LENGTH="30" NAME="title" TYPE="id_String" NULLABLE="true" READONLY="true"/>
      		<COLUMN KEY="false" LENGTH="100" NAME="link" TYPE="id_String" NULLABLE="true" READONLY="true"/>
      		<COLUMN KEY="false" NAME="updated" TYPE="id_String" NULLABLE="true" READONLY="true"/>
      		<COLUMN KEY="false" LENGTH="200" NAME="summary" TYPE="id_String" NULLABLE="true" READONLY="true"/>
      	</TABLE>
     </PARAMETER>
  </PARAMETERS>
 <CODEGENERATION>
    <IMPORTS>
       <IMPORT NAME="Java-DOM4J" MODULE="dom4j-1.6.1.jar" MVN="mvn:org.talend.libraries/dom4j-1.6.1/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.dom4j-jaxen/lib/dom4j-1.6.1.jar" REQUIRED="true" BundleID="" />
        <IMPORT NAME="Java-JAXEN" MODULE="jaxen-1.1.1.jar" MVN="mvn:org.talend.libraries/jaxen-1.1.1/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.dom4j-jaxen/lib/jaxen-1.1.1.jar" REQUIRED="true" BundleID="" />
     </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
  </RETURNS>

</COMPONENT>
