<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
	PARTITIONING="NONE"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>File/Output</FAMILY>
  </FAMILIES>
  
  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
  
<CONNECTORS>
	<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
	<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
	<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
	<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
	<CONNECTOR CTYPE="COMPONENT_OK" />
	<CONNECTOR CTYPE="COMPONENT_ERROR" />
	<CONNECTOR CTYPE="RUN_IF" />
</CONNECTORS>
  
  <PARAMETERS>
    
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="10"
      READONLY="true"
    >
      <TABLE READONLY="true">
        <COLUMN NAME="key" TYPE="id_String" LENGTH="255" READONLY="true"/>
        <COLUMN NAME="value" TYPE="id_String" LENGTH="255" READONLY="true"/>
      </TABLE>
    </PARAMETER>

    <PARAMETER NAME="FILE_FORMAT" FIELD="CLOSED_LIST" NUM_ROW="15" REQUIRED="false">
      <ITEMS DEFAULT="PROPERTIES_FORMAT">
		<ITEM NAME="PROPERTIES_FORMAT" VALUE="PROPERTIES_FORMAT" />
		<ITEM NAME="INI_FORMAT" VALUE="INI_FORMAT" />
	  </ITEMS>
    </PARAMETER>
    
    <PARAMETER NAME="SECTION_NAME" FIELD="TEXT" NUM_ROW="16" REQUIRED="true" SHOW_IF="(FILE_FORMAT=='INI_FORMAT')">
    	<DEFAULT>"section"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="APPEND" FIELD="CHECK" NUM_ROW="17" />

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="20"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.properties"</DEFAULT>
    </PARAMETER>
    
    
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
    <PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="10" REQUIRED="true" REPOSITORY_VALUE="ENCODING">
	  <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>
  
  <CODEGENERATION>
	<IMPORTS>
		<IMPORT NAME="Java_Ini4j-0.5.1" MODULE="ini4j-0.5.1.jar" MVN="mvn:org.talend.libraries/ini4j-0.5.1/6.0.0"  REQUIRED="true" />
	</IMPORTS>
  </CODEGENERATION>
  
  <RETURNS>
	<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
  </RETURNS>

</COMPONENT>
