<COMPONENT>

	<HEADER
		PLATEFORM="ALL"
		SERIAL=""
		VERSION="0.102"
		STATUS="ALPHA"
		COMPATIBILITY="ALL"
		AUTHOR="Talend"
		RELEASE_DATE="20100112A"
		STARTABLE="true"
		COMBINE="tELTNode"
		PARTITIONING="NONE"
	>
		<SIGNATURE/>
	</HEADER>

	<FAMILIES>
		<FAMILY>ELT/Combined SQL</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL/>
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
		<CONNECTOR NAME="ELTCOMBINE" CTYPE="FLOW" MAX_OUTPUT="1" COLOR="086438" BASE_SCHEMA="FLOW" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>

	<PARAMETERS>
		
		<PARAMETER
			NAME="TABLE_NAME"
			FIELD="TEXT"
			NUM_ROW="10"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="SCHEMA_NAME"
			FIELD="TEXT"
			NUM_ROW="10"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER
			NAME="SCHEMA"
			FIELD="SCHEMA_TYPE"
			NUM_ROW="20"
			CONTEXT="FLOW"
		/>
		
		<PARAMETER 
			NAME="COMBINE_SCHEMA" 
			FIELD="SCHEMA_TYPE" 
			NUM_ROW="25"
			SHOW="false"
			CONTEXT="ELTCOMBINE"
		/>

		<PARAMETER 
			NAME="HAS_ADD_COLS" 
			FIELD="CHECK" 
			NUM_ROW="30"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER
			NAME="ADD_COLS"
			FIELD="TABLE"
			REQUIRED="false" 
			NUM_ROW="40"
			NB_LINES="3"
			SHOW_IF="HAS_ADD_COLS=='true'"
		>
			<ITEMS>
				<ITEM NAME="NAME" VALUE='COLUMN_NAME'/>	
				<ITEM NAME="SQL" VALUE='"now()"' />
				<!--
				<ITEM NAME="POS" FIELD="CLOSED_LIST">
					<ITEMS DEFAULT="BEFORE">
						<ITEM NAME="BEFORE" VALUE="BEFORE"/>
						<ITEM NAME="AFTER" VALUE="AFTER"/>
						<ITEM NAME="REPLACE" VALUE="REPLACE"/>
					</ITEMS>
				</ITEM>
				<ITEM NAME="REFCOL" FIELD="COLUMN_LIST" />
				-->
			</ITEMS>
		</PARAMETER>
	
	</PARAMETERS>

	<ADVANCED_PARAMETERS />
	
	<CODEGENERATION>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="FLOW"/>
		<RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW"/>
	</RETURNS>
</COMPONENT>