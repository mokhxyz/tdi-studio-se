<!-- $Id: tAlfrescoOutput_java.xml,v 1.6 2008/10/10 11:43:19 mdutoo Exp $ -->
<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="1.0" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Open Wide" RELEASE_DATE="20081006A"
		STARTABLE="false" DATA_AUTO_PROPAGATE="false" HAS_CONDITIONAL_OUTPUTS="true" EXTENSION="org.talend.designer.alfrescooutput" PARTITIONING="AUTO">
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>Business/Alfresco (Deprecated)</FAMILY>
  </FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR NAME="REJECT" NOT_SHOW_IF="(DIE_ON_ERROR == 'true')" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"
			LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="ALFRESCO_SERVER_URL" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="1" GROUP="ALFRESCO_SERVER_GROUP">
			<DEFAULT>"http://localhost:8080/alfresco"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ALFRESCO_USER_LOGIN" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="2" GROUP="ALFRESCO_SERVER_GROUP">
			<DEFAULT>"admin"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ALFRESCO_USER_PASSWORD" FIELD="PASSWORD" REQUIRED="true"
			NUM_ROW="2" GROUP="ALFRESCO_SERVER_GROUP">
			<DEFAULT>"admin"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_LOCATION_BASE" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="3" GROUP="TARGET_LOCATION_GROUP">
			<DEFAULT></DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MAP_TARGET_LOCATION_FROM_COLUMN" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="3" GROUP="TARGET_LOCATION_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_LOCATION_COLUMN" FIELD="COLUMN_LIST" REQUIRED="false"
			NUM_ROW="3" GROUP="TARGET_LOCATION_GROUP" SHOW_IF="MAP_TARGET_LOCATION_FROM_COLUMN == 'true'">
		</PARAMETER>

		<PARAMETER NAME="DOCUMENT_MODE" FIELD="CLOSED_LIST" REQUIRED="true"
			NUM_ROW="4" GROUP="CREATE_OR_UPDATE_MODE_GROUP">
			<ITEMS DEFAULT="CREATE_ONLY">
				<ITEM NAME="CREATE_ONLY" VALUE="Create only" />
				<ITEM NAME="CREATE_OR_UPDATE" VALUE="Create or update"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="CONTAINER_MODE" FIELD="CLOSED_LIST" REQUIRED="true"
			NUM_ROW="4" GROUP="CREATE_OR_UPDATE_MODE_GROUP">
			<ITEMS DEFAULT="CREATE_OR_UPDATE">
				<ITEM NAME="CREATE_OR_UPDATE" VALUE="Create or update" />
				<ITEM NAME="UPDATE_ONLY" VALUE="Update only"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="DEFINE_DOCUMENT_TYPE" FIELD="EXTERNAL" NUM_ROW="5">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="AVAILABLE_ALFRESCO_MODELS" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="5" NB_LINES="3" GROUP="ALFRESCO_TYPE_GROUP" SHOW="false">
			<ITEMS>
				<ITEM NAME="FILEPATH" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="AVAILABLE_ALFRESCO_NAMESPACES" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="5" NB_LINES="3" GROUP="ALFRESCO_TYPE_GROUP" SHOW="false">
			<ITEMS>
				<ITEM NAME="PREFIX" FIELD="TEXT" />
				<ITEM NAME="URI" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="ALFRESCO_TYPE_NAME" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="5" GROUP="ALFRESCO_TYPE_GROUP" SHOW="false">
			<DEFAULT>cm:content</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ALFRESCO_ASPECT_NAMES" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="5" NB_LINES="5" GROUP="ALFRESCO_TYPE_GROUP" SHOW="false">
			<ITEMS>
				<ITEM NAME="NAME" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="PROPERTY_MAPPING" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="10" NB_LINES="10" GROUP="PROPERTY_MAPPING_GROUP" SHOW="true">
			<ITEMS>
				<ITEM NAME="NAME" FIELD="TEXT" READONLY="true" />
				<ITEM NAME="TITLE" FIELD="TEXT" READONLY="true" />
				<ITEM NAME="TYPE" FIELD="TEXT" READONLY="true" />
				<ITEM NAME="MANDATORY" FIELD="CHECK" READONLY="true" />
				<ITEM NAME="DEFAULT" FIELD="TEXT" READONLY="true" />
				<ITEM NAME="COLUMN" FIELD="COLUMN_LIST" />
				<ITEM NAME="VALUE" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="21">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="SCHEMA_REJECT" FIELD="SCHEMA_TYPE"
			REQUIRED="true" NUM_ROW="22" CONTEXT="REJECT" SHOW="true">
			<TABLE READONLY="true">
				<COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255"
					READONLY="false" CUSTOM="true" />
				<COLUMN NAME="errorMessage" TYPE="id_String"
					LENGTH="255" READONLY="false" CUSTOM="true" />
			</TABLE>
		</PARAMETER>

		<PARAMETER NAME="RESULT_LOG_FILENAME" FIELD="FILE" NUM_ROW="36"
			REQUIRED="true">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.xml"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER
      		NAME="DIE_ON_ERROR"
      		FIELD="CHECK"
      		NUM_ROW="90"
    	>
      		<DEFAULT>false</DEFAULT>
    	</PARAMETER>
		
	</PARAMETERS>

	<ADVANCED_PARAMETERS>

		<PARAMETER NAME="CONFIGURE_TARGET_LOCATION_CONTAINER" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="1" GROUP="TARGET_LOCATION_CONTAINER_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_LOCATION_CONTAINER_TYPE" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="2" GROUP="TARGET_LOCATION_CONTAINER_GROUP" SHOW_IF="CONFIGURE_TARGET_LOCATION_CONTAINER == 'true'">
			<DEFAULT>cm:folder</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TARGET_LOCATION_CHILD_ASSOCIATION_TYPE" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="2" GROUP="TARGET_LOCATION_CONTAINER_GROUP" SHOW_IF="CONFIGURE_TARGET_LOCATION_CONTAINER == 'true'">
			<DEFAULT>cm:contains</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONFIGURE_PERMISSIONS" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="3" GROUP="PERMISSIONS_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PERMISSIONS_TARGET" FIELD="CLOSED_LIST" REQUIRED="true"
			NUM_ROW="3" GROUP="PERMISSIONS_GROUP" SHOW_IF="CONFIGURE_PERMISSIONS == 'true'">
			<ITEMS DEFAULT="CONTAINER">
				<ITEM NAME="CONTAINER" VALUE="Container" />
				<ITEM NAME="DOCUMENT" VALUE="Document"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="INHERIT_PERMISSIONS" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="4" GROUP="PERMISSIONS_GROUP" SHOW_IF="CONFIGURE_PERMISSIONS == 'true'">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PERMISSIONS" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="5" NB_LINES="3" GROUP="PERMISSIONS_GROUP" SHOW_IF="CONFIGURE_PERMISSIONS == 'true'">
			<ITEMS>
				<ITEM NAME="USERORGROUP" FIELD="TEXT"/>
				<ITEM NAME="USERORGROUPCOLUMN" FIELD="COLUMN_LIST"/>
				<ITEM NAME="PERMISSION" FIELD="TEXT" VALUE="Collaborator" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="ASSOCIATION_MAPPING" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="21" NB_LINES="10" GROUP="ASSOCIATION_MAPPING_GROUP" SHOW="true">
			<ITEMS>
				<ITEM NAME="NAME" FIELD="TEXT" READONLY="true" />
				<ITEM NAME="CHILD" FIELD="CHECK" READONLY="true" />
				<ITEM NAME="TITLE" FIELD="TEXT" READONLY="true" />
				<ITEM NAME="TYPE" FIELD="TEXT" READONLY="true" />
				<ITEM NAME="MANDATORY" FIELD="CHECK" READONLY="true" />
				<ITEM NAME="MANY" FIELD="CHECK" READONLY="true" />
				<ITEM NAME="COLUMN" FIELD="COLUMN_LIST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="REST_ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="20"
			REQUIRED="true">
			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>

	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="commons-logging" MODULE="commons-logging-1.0.4.jar" MVN="mvn:org.talend.libraries/commons-logging-1.0.4/6.0.0"  UrlPath="platform:/base/plugins/org.apache.commons.logging_1.0.4.v201101211617.jar"
				REQUIRED="true" />
			<IMPORT NAME="commons-codec" MODULE="commons-codec-1.6.jar" MVN="mvn:org.talend.libraries/commons-codec-1.6/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-codec-1.6.jar"
				REQUIRED="true" />
			<IMPORT NAME="commons-httpclient" MODULE="commons-httpclient-3.0.jar" MVN="mvn:org.talend.libraries/commons-httpclient-3.0/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.apache.http/lib/commons-httpclient-3.0.jar"
				REQUIRED="true" />
			<IMPORT NAME="stax-api" MODULE="stax-api-1.0.1.jar" MVN="mvn:org.talend.libraries/stax-api-1.0.1/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.esb/lib/stax-api-1.0.1.jar"
				REQUIRED="true" />
			<IMPORT NAME="woodstox" MODULE="wstx-lgpl-3.2.7.jar" MVN="mvn:org.talend.libraries/wstx-lgpl-3.2.7/6.0.0" 
				REQUIRED="true" />
			<IMPORT NAME="talendalfresco-client" MODULE="talendalfresco-client_1.0.jar" MVN="mvn:org.talend.libraries/talendalfresco-client_1.0/6.0.0" 
				REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
		<RETURN NAME="NB_LINE_REJECTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
	</RETURNS>

</COMPONENT>
