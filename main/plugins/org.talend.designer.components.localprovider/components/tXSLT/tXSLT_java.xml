<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
	PARTITIONING="NONE"
  >
    <SIGNATURE />
  </HEADER>

  <FAMILIES>
    <FAMILY>XML</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>

   <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="XML_FILE"
      FIELD="FILE"
      NUM_ROW="1"
      REQUIRED="true"
    >
    <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.xml"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="XSLT_FILE"
      FIELD="FILE"
      NUM_ROW="2"
      REQUIRED="true"
    > 
	<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.xsl"</DEFAULT>  
    </PARAMETER>
    <PARAMETER
      NAME="OUTPUT_FILE"
      FIELD="FILE"
      NUM_ROW="3"
      REQUIRED="true"      
     >
     <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.html"</DEFAULT>
    </PARAMETER>
	
	<PARAMETER NAME="PARAMS" FIELD="TABLE" NUM_ROW="6" NB_LINES="8">
      <ITEMS>
   		<ITEM NAME="NAME"/>
        <ITEM NAME="VALUE"/>
      </ITEMS>
    </PARAMETER>

  </PARAMETERS>
  
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="SAXON9" MODULE="saxon9.jar" MVN="mvn:org.talend.libraries/saxon9/6.0.0"  REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>
	
  <RETURNS>
  	<RETURN NAME="OUTPUT_FILEPATH" TYPE="id_String" AVAILABILITY="FLOW"/>
  	<RETURN NAME="OUTPUT_FILENAME" TYPE="id_String" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>
