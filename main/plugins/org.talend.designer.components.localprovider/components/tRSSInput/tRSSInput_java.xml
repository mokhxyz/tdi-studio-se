<!-- $Id: tRSSInput_java.xml 696 2006-11-23 05:45:10Z nrousseau $ -->
<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Internet</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
   <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      READONLY="true" 
      REQUIRED="true"
      NUM_ROW="1"
    >
      <TABLE READONLY="true">
        <COLUMN NAME="TITLE" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      	<COLUMN NAME="DESCRIPTION" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
       	<COLUMN NAME="PUBDATE" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
       	 <COLUMN NAME="LINK" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>
    <PARAMETER
      NAME="RSS"
      FIELD="TEXT"
      NUM_ROW="2"
      REQUIRED="true"
    >
    <DEFAULT>"http://feeds.feedburner.com/Talend"</DEFAULT>
    </PARAMETER>

     <PARAMETER
      NAME="ARTICLE_DATE"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="3"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER
      NAME="DATE"
      FIELD="DATE"
      REQUIRED="true"
      NUM_ROW="3"
      SHOW_IF="ARTICLE_DATE == 'true'"
    >
    <DEFAULT>"2007-05-30 08:12:12"</DEFAULT>
    </PARAMETER>

     <PARAMETER
      NAME="ARTICLE_AMOUNT"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="4"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="AMOUNT"
      FIELD="TEXT"
      REQUIRED="true"
      NUM_ROW="4"
      SHOW_IF="ARTICLE_AMOUNT == 'true'"
    >
    <DEFAULT>10</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="5"
    >
    <DEFAULT>false</DEFAULT>
    </PARAMETER>
  </PARAMETERS>

  <ADVANCED_PARAMETERS>

   <PARAMETER
       NAME="USE_PROXY"
       FIELD="CHECK"
       REQUIRED="true"
	   NUM_ROW="10"
	   REPOSITORY_VALUE="UES_PROXY">
	   <DEFAULT>false</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="PROXY_HOST"
		FIELD="TEXT"
		NUM_ROW="20"
		SHOW_IF="USE_PROXY == 'true'"
		REPOSITORY_VALUE="PROXY_HOST">
		<DEFAULT>"61.163.92.4"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="PROXY_PORT"
		FIELD="TEXT"
		NUM_ROW="30"
		SHOW_IF="USE_PROXY == 'true'"
		REPOSITORY_VALUE="PROXY_PORT">
		<DEFAULT>"1080"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="PROXY_USERNAME"
		FIELD="TEXT"
		NUM_ROW="40"
		SHOW_IF="USE_PROXY == 'true'"
		REPOSITORY_VALUE="PROXY_USERNAME">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="PROXY_PASSWORD"
		FIELD="PASSWORD"
		NUM_ROW="50"
		SHOW_IF="USE_PROXY == 'true'"
		REPOSITORY_VALUE="PROXY_PASSWORD">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
    <IMPORTS>
       <IMPORT NAME="Java-Feed" MODULE="feed4j.jar" MVN="mvn:org.talend.libraries/feed4j/6.0.0"  REQUIRED="true" />
       <IMPORT NAME="Java-DOM" MODULE="dom4j-1.6.1.jar" MVN="mvn:org.talend.libraries/dom4j-1.6.1/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.dom4j-jaxen/lib/dom4j-1.6.1.jar" REQUIRED="true" BundleID="" />
        <IMPORT NAME="Java-NEKO" MODULE="nekohtml.jar" MVN="mvn:org.talend.libraries/nekohtml/6.0.0"  REQUIRED="true" />
       <IMPORT NAME="Java-XER" MODULE="xercesImpl.jar" MVN="mvn:org.talend.libraries/xercesImpl/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.apache.xml/lib/xercesImpl.jar" REQUIRED="true" BundleID="" />
       <IMPORT NAME="Java-XML" MODULE="xml-apis.jar" MVN="mvn:org.talend.libraries/xml-apis/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.xml/lib/xml-apis.jar" REQUIRED="true" />

     </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
