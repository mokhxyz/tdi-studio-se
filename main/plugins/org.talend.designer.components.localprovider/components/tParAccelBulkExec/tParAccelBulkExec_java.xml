<COMPONENT>

	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070320A"
		STARTABLE="true" LOG4J_ENABLED="true">
		<SIGNATURE />
	</HEADER>

  <FAMILIES>
    <FAMILY>Databases/ParAccel</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="RUN_IF" MAX_OUTPUT="1" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="1" REPOSITORY_VALUE="DATABASE:PARACCEL"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"/>

    	<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
      		NUM_ROW="2">
      		<DEFAULT>false</DEFAULT>
    	</PARAMETER>

	    <PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" REQUIRED="true" FILTER="tParAccelConnection"
	      NUM_ROW="2" SHOW_IF="USE_EXISTING_CONNECTION == 'true'" DYNAMIC_SETTINGS="true"/>

		<PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="3"
			REPOSITORY_VALUE="SERVER_NAME"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
		      NAME="TYPE"
		      FIELD="TEXT"
		      NUM_ROW="3"
		      SHOW="false"
		      REPOSITORY_VALUE="TYPE"
		    >
		      <DEFAULT>ParAccel</DEFAULT>
	    </PARAMETER>

		<PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="3"
			REPOSITORY_VALUE="PORT"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>"5439"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DBNAME" FIELD="TEXT" NUM_ROW="4"
			REPOSITORY_VALUE="SID"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA_DB" FIELD="TEXT" NUM_ROW="4"
			REPOSITORY_VALUE="SCHEMA"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USER" FIELD="TEXT" NUM_ROW="5"
			REPOSITORY_VALUE="USERNAME"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASS" FIELD="PASSWORD" NUM_ROW="5"
			REPOSITORY_VALUE="PASSWORD"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TABLE" FIELD="DBTABLE" NUM_ROW="6">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TABLE_ACTION" FIELD="CLOSED_LIST"
			NUM_ROW="7" NOT_SHOW_IF="PARALLELIZE == 'true'">
			<ITEMS DEFAULT="NONE">
				<ITEM NAME="NONE" VALUE="NONE" />
				<ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE" />
				<ITEM NAME="CREATE" VALUE="CREATE" />
				<ITEM NAME="CREATE_IF_NOT_EXISTS"
					VALUE="CREATE_IF_NOT_EXISTS" />
				<ITEM NAME="DROP_IF_EXISTS_AND_CREATE" VALUE="DROP_IF_EXISTS_AND_CREATE" />
				<ITEM NAME="CLEAR" VALUE="CLEAR" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="NOTE" FIELD="LABEL" NUM_ROW="7"
				NOT_SHOW_IF="(TABLE_ACTION == 'NONE') OR (USE_EXISTING_CONNECTION == 'false')"
			>
			<DEFAULT>Warning : this component configuration will automatically generate a commit before bulk insert</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="12">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MAPPING" FIELD="MAPPING_TYPE" NUM_ROW="16"
			SHOW="false">
			<DEFAULT>postgres_id</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="COPY_MODE" FIELD="CLOSED_LIST" NUM_ROW="6">
			<ITEMS DEFAULT="BASIC">
				<ITEM NAME="BASIC" VALUE="BASIC" />
				<ITEM NAME="PARALLEL" VALUE="PARALLEL" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="7"
			REQUIRED="true">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.csv"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FILETYPE" FIELD="CLOSED_LIST" NUM_ROW="8">
			<ITEMS DEFAULT="TXTFILE">
				<ITEM NAME="TXTFILE" VALUE="TXTFILE" />
				<ITEM NAME="GZIP" VALUE="GZIP" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="FIELD_LAYOUT" FIELD="CLOSED_LIST" NUM_ROW="8">
			<ITEMS DEFAULT="DELIMITED">
				<ITEM NAME="DELIMITED" VALUE="DELIMITED" />
				<ITEM NAME="FIXEDWIDTH" VALUE="FIXEDWIDTH" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="METHOD_FIXED_WIDTH" FIELD="CHECK" NUM_ROW="9"
			SHOW_IF="(FIELD_LAYOUT=='FIXEDWIDTH')" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FIELDS_TERMINATED_BY" FIELD="TEXT" NUM_ROW="10" MAX_LENGTH="1"
			SHOW_IF="(FIELD_LAYOUT=='DELIMITED')" >
			<DEFAULT>";"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FIXED_WIDTH" FIELD="TEXT" NUM_ROW="10"
			SHOW_IF="(FIELD_LAYOUT=='FIXEDWIDTH' and METHOD_FIXED_WIDTH == 'true')" >
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
	  	<PARAMETER NAME="EXPLICIT_IDS" FIELD="CHECK" NUM_ROW="11">
			<DEFAULT>"false"</DEFAULT>
		</PARAMETER>
	  	<PARAMETER NAME="REMOVE_QUOTES" FIELD="CHECK" NUM_ROW="11"
	  		SHOW_IF="(FIELD_LAYOUT=='DELIMITED')" >
			<DEFAULT>"false"</DEFAULT>
		</PARAMETER>

	  	<PARAMETER NAME="MAX_ERRORS" FIELD="TEXT" NUM_ROW="12">
			<DEFAULT>"2000000000"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DATE_FORMAT" FIELD="TEXT" NUM_ROW="13">
			<DEFAULT>"YYYY-MM-DD"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TIME_FORMAT" FIELD="TEXT" NUM_ROW="14">
			<DEFAULT>"YYYY-MM-DD HH:MI:SS"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ADDL_OPTIONS" FIELD="TEXT" NUM_ROW="15">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="LOG_FILE"
      		FIELD="FILE"
      		NUM_ROW="20"
      		REQUIRED="true"
    	>
      		<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/paraccel-jdbc.log"</DEFAULT>
    	</PARAMETER>

		<PARAMETER NAME="LOG_LEVEL" FIELD="CLOSED_LIST" NUM_ROW="25">
			<ITEMS DEFAULT="INFO">
				<ITEM NAME="NONE" VALUE="0" />
				<ITEM NAME="INFO" VALUE="1" />
				<ITEM NAME="DEBUG" VALUE="2" />
				<ITEM NAME="PROTOCOL" VALUE="4" />
				<ITEM NAME="INFO_DEBUG" VALUE="3" />
				<ITEM NAME="INFO_PROTOCOL" VALUE="5" />
				<ITEM NAME="DEBUG_PROTOCOL" VALUE="6" />
				<ITEM NAME="ALL" VALUE="7" />
			</ITEMS>
		</PARAMETER>

	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="Driver-ParAccel" MODULE="paraccel-jdbc.jar" MVN="mvn:org.talend.libraries/paraccel-jdbc/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.jdbc.paraccel/lib/paraccel-jdbc.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS />
</COMPONENT>
