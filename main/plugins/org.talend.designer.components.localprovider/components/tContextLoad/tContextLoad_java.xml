<COMPONENT>
   <HEADER 
     PLATEFORM="ALL" 
     SERIAL="" 
     VERSION="0.101" 
     STATUS="ALPHA" 
     COMPATIBILITY="ALL" 
     AUTHOR="Talend" 
     RELEASE_DATE="20050320A" 
     STARTABLE="false"
     SCHEMA_AUTO_PROPAGATE="false"
	   PARTITIONING="AUTO"
     LOG4J_ENABLED="true"
	 >
      <SIGNATURE></SIGNATURE>
   </HEADER>
   
  <FAMILIES>
    <FAMILY>Misc</FAMILY>
  </FAMILIES>    
   
   <DOCUMENTATION>
      <URL/>
   </DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>
   <PARAMETERS>
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      READONLY="true" 
      REQUIRED="true"
      NUM_ROW="1"
    >
      <TABLE READONLY="true">
        <COLUMN NAME="key" TYPE="id_String" READONLY="true" CUSTOM="true" LENGTH="255"/>
        <COLUMN NAME="value" TYPE="id_String" READONLY="false" CUSTOM="true" LENGTH="255"/>
      </TABLE>
    </PARAMETER>

    <PARAMETER
	  NAME="LOAD_NEW_VARIABLE"
	  FIELD="CLOSED_LIST"
	  REQUIRED="false"
	  NUM_ROW="2">
	  <ITEMS DEFAULT="WARNING">
		<ITEM NAME="ERROR" VALUE="Error" />
		<ITEM NAME="WARNING" VALUE="Warning" />
		<ITEM NAME="INFO" VALUE="Info" />
	  </ITEMS>
	</PARAMETER>
	
	<PARAMETER
	  NAME="NOT_LOAD_OLD_VARIABLE"
	  FIELD="CLOSED_LIST"
	  REQUIRED="false"
	  NUM_ROW="3">
	  <ITEMS DEFAULT="WARNING">
		<ITEM NAME="ERROR" VALUE="Error" />
		<ITEM NAME="WARNING" VALUE="Warning" />
		<ITEM NAME="INFO" VALUE="Info" />
	  </ITEMS>
	</PARAMETER>
	
    <PARAMETER
      NAME="PRINT_OPERATIONS"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="4"
      DYNAMIC_SETTINGS="true"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="DISABLE_ERROR"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="5"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    

	<PARAMETER
      NAME="DISABLE_WARNINGS"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="5"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DISABLE_INFO"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="5"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>    
    
    <PARAMETER
      NAME="DIEONERROR"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="10"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>    
    
   </PARAMETERS>
   <CODEGENERATION/>
   <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="KEY_NOT_INCONTEXT" TYPE="id_String" AVAILABILITY="AFTER"/>
      <RETURN NAME="KEY_NOT_LOADED" TYPE="id_String" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
