<!-- $Id: tWarn_perl.xml 1528 2007-01-19 17:03:36Z smallet $ -->
<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
       SERIAL=""
      VERSION="0.101"
       STATUS="ALPHA"
         
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
	  PARTITIONING="AUTO"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Logs_Errors</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="RUN_IF" MAX_INPUT="0"/>
  </CONNECTORS>

  <PARAMETERS>
  	<PARAMETER NAME="USEROWLABEL" FIELD="CHECK" NUM_ROW="1">
    	<DEFAULT>true</DEFAULT>
    </PARAMETER>
  
    <PARAMETER NAME="METTERLABEL" FIELD="TEXT" NUM_ROW="2" SHOW_IF="USEROWLABEL=='false'">
        <DEFAULT>"MyLabel"</DEFAULT>
    </PARAMETER>
       
    <PARAMETER NAME="ABSOLUTE" FIELD="CLOSED_LIST" NUM_ROW="3" REQUIRED="true">
    	<ITEMS DEFAULT="Absolute">
    		<ITEM NAME="Absolute" VALUE="Absolute" />
    		<ITEM NAME="Relative" VALUE="Relative" />
    	</ITEMS>
    </PARAMETER>
    
    <PARAMETER NAME="CONNECTIONS" FIELD="CONNECTION_LIST" NUM_ROW="4" SHOW_IF = "ABSOLUTE=='Relative'"  FILTER="TYPE:FLOW_MAIN|FLOW_MERGE">
    </PARAMETER>
   
    <PARAMETER NAME="THRESHLODS" FIELD="TABLE" NUM_ROW="5" NB_LINES="5" >
      <ITEMS>
        <ITEM NAME="LABEL" RAW="true" VALUE="newline"/>
        <ITEM NAME="BOTTOM" VALUE="0"/>
        <ITEM NAME="TOP" VALUE="80"/>
        <ITEM NAME="COLOR" FIELD="COLOR"/>
      </ITEMS>
    </PARAMETER>
   
   </PARAMETERS>

   <CODEGENERATION/>

   <RETURNS/>

</COMPONENT>
