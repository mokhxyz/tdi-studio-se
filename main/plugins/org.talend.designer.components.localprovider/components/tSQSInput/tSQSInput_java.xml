<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20130810A"
    STARTABLE="true"
    PARTITIONING="NONE"
    LOG4J_ENABLED="true"
    SUPPORTS_DB_TYPE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Cloud/Amazon/SQS</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

    <PARAMETERS>
		<PARAMETER
			NAME="USE_EXISTING_CONNECTION"
			FIELD="CHECK"
			NUM_ROW="2"
			GROUP="CONNECTION_CONFIG"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER
			NAME="CONNECTION"
			FIELD="COMPONENT_LIST"
			REQUIRED="true"
			FILTER="tSQSConnection"
			NUM_ROW="2"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
			GROUP="CONNECTION_CONFIG" />
	
		<PARAMETER
			NAME="ACCESS_KEY"
			FIELD="TEXT"
			NUM_ROW="20"
			REQUIRED="true"
			GROUP="CONNECTION_CONFIG"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (INHERIT_CREDENTIALS == 'false')"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
	
		<PARAMETER
			NAME="SECRET_KEY"
			FIELD="PASSWORD"
			NUM_ROW="20"
			REQUIRED="true"
			GROUP="CONNECTION_CONFIG"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (INHERIT_CREDENTIALS == 'false')"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
	
		<PARAMETER
			NAME="INHERIT_CREDENTIALS"
			FIELD="CHECK"
			NUM_ROW="21"
			REQUIRED="true"
			GROUP="CONNECTION_CONFIG"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER
      		NAME="ASSUME_ROLE"
      		FIELD="CHECK"
      		NUM_ROW="22"
      		REQUIRED="true"
      		GROUP="CONNECTION_CONFIG"
      		SHOW_IF="(USE_EXISTING_CONNECTION == 'false')" 
    	>
      		<DEFAULT>false</DEFAULT>
    	</PARAMETER>
    
    	<PARAMETER
      		NAME="ARN"
      		FIELD="TEXT"
      		NUM_ROW="23"
      		REQUIRED="true"
      		GROUP="CONNECTION_CONFIG"
      		SHOW_IF="isShow[ASSUME_ROLE] AND (ASSUME_ROLE == 'true')"
    	>
      		<DEFAULT>""</DEFAULT>
    	</PARAMETER>
    
    	<PARAMETER
      		NAME="ROLE_SESSION_NAME"
      		FIELD="TEXT"
      		NUM_ROW="24"
      		REQUIRED="true"
      		GROUP="CONNECTION_CONFIG"
      		SHOW_IF="isShow[ASSUME_ROLE] AND (ASSUME_ROLE == 'true')"
    	>
      		<DEFAULT>""</DEFAULT>
    	</PARAMETER>
    
    	<PARAMETER
      		NAME="SESSION_DURATION"
      		FIELD="TEXT"
      		NUM_ROW="25"
      		REQUIRED="true"
      		GROUP="CONNECTION_CONFIG"
      		SHOW_IF="isShow[ASSUME_ROLE] AND (ASSUME_ROLE == 'true')"
    	>
      		<DEFAULT>15</DEFAULT>
    	</PARAMETER>
		
	    <PARAMETER 
		    NAME="REGION" 
		    FIELD="OPENED_LIST" 
		    RAW="true" 
		    NUM_ROW="26" 
		    REQUIRED="true" 
		    SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
		 >
	     	<ITEMS DEFAULT="DEFAULT">
	        	<ITEM NAME="DEFAULT" VALUE="DEFAULT" />
	         	<ITEM NAME="AP_SOUTHEAST_1" VALUE="&quot;ap-southeast-1&quot;" />
		        <ITEM NAME="AP_SOUTHEAST_2" VALUE="&quot;ap-southeast-2&quot;" />
		        <ITEM NAME="AP_NORTHEAST_1" VALUE="&quot;ap-northeast-1&quot;" />
		        <ITEM NAME="CN_NORTH_1" VALUE="&quot;cn-north-1&quot;" />
		        <ITEM NAME="EU_WEST_1" VALUE="&quot;eu-west-1&quot;" />
		        <ITEM NAME="EU_CENTRAL_1" VALUE="&quot;eu-central-1&quot;" />
		        <ITEM NAME="GovCloud" VALUE="&quot;us-gov-west-1&quot;" />
		        <ITEM NAME="SA_EAST_1" VALUE="&quot;sa-east-1&quot;" />
		        <ITEM NAME="US_EAST_1" VALUE="&quot;us-east-1&quot;" />
		        <ITEM NAME="US_WEST_1" VALUE="&quot;us-west-1&quot;" />
		        <ITEM NAME="US_WEST_2" VALUE="&quot;us-west-2&quot;" />
	    	</ITEMS>
	    </PARAMETER>
	
		<PARAMETER
			NAME="QUEUE_URL"
			FIELD="TEXT"
			NUM_ROW="30"
			REQUIRED="true"
		>
			<DEFAULT>""</DEFAULT>
	    </PARAMETER>
	    
	    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="50" REQUIRED="true">
			<TABLE>
				<COLUMN KEY="true" NAME="MessageId" TYPE="id_String" />
				<COLUMN KEY="false" NAME="ReceiptHandle" TYPE="id_String" />
				<COLUMN KEY="false" NAME="MD5OfBody" TYPE="id_String" />
				<COLUMN KEY="false" NAME="Body" TYPE="id_String" />
			</TABLE>
		</PARAMETER>
	
		<PARAMETER
			NAME="GET_ATTRIBUTES"
			FIELD="CHECK"
			NUM_ROW="60"
			REQUIRED="true"
		>
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
	
		<PARAMETER
			NAME="GET_MESSAGE_ATTRIBUTES"
			FIELD="CHECK"
			NUM_ROW="70"
			REQUIRED="true"
		>
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
	
		<PARAMETER
			NAME="VISIBILITY_TIMEOUT"
			FIELD="CHECK"
			NUM_ROW="80"
			REQUIRED="true"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	
		<PARAMETER
			NAME="VISIBILITY_TIMEOUT_VALUE"
			FIELD="TEXT"
			NUM_ROW="80"
			REQUIRED="true"
			SHOW_IF="VISIBILITY_TIMEOUT == 'true'"
		>
			<DEFAULT>30</DEFAULT>
		</PARAMETER>
		
		<PARAMETER
			NAME="CUSTOM_WAIT_TIME"
			FIELD="CHECK"
			NUM_ROW="85"
			REQUIRED="true"
		>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	
		<PARAMETER
			NAME="WAIT_TIME"
			FIELD="TEXT"
			NUM_ROW="85"
			REQUIRED="true"
			SHOW_IF="CUSTOM_WAIT_TIME == 'true'"
		>
			<DEFAULT>10</DEFAULT>
		</PARAMETER>
	
		<PARAMETER
			NAME="DELETE_MESSAGE"
			FIELD="CHECK"
			NUM_ROW="90"
			REQUIRED="true"
		>
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
	
		<PARAMETER
			NAME="READ_ALL_MESSAGE"
			FIELD="CHECK"
			NUM_ROW="100"
			REQUIRED="true"
			SHOW_IF="DELETE_MESSAGE == 'true'"
		>
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
		
		<PARAMETER 
			NAME="MAX_NUMBER_MESSAGES" 
			FIELD="TEXT" 
			NUM_ROW="105"
			REQUIRED="true" 
			SHOW_IF="(READ_ALL_MESSAGE == 'false' OR DELETE_MESSAGE == 'false')"
		>
			<DEFAULT>1</DEFAULT>
		</PARAMETER>
	
		<PARAMETER
			NAME="DIE_ON_ERROR"
			FIELD="CHECK"
			NUM_ROW="121"
		>
			<DEFAULT>true</DEFAULT>
		</PARAMETER>
    
   </PARAMETERS>

    <ADVANCED_PARAMETERS>
	    <PARAMETER
	      NAME="CONFIG_CLIENT"
	      FIELD="CHECK"
	      NUM_ROW="10"
	      NB_LINES="5"
	      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
	    >
	    </PARAMETER>
	
	    <PARAMETER
	      NAME="CLIENT_CONFIGURATION"
	      FIELD="TABLE"
	      NUM_ROW="30"
	      NB_LINES="5"
	      REQUIRED="true"
	      SHOW_IF="(CONFIG_CLIENT =='true') AND (USE_EXISTING_CONNECTION == 'false')"
	    >
	      <ITEMS>
	        <ITEM NAME="CLIENT_PARAMETER" FIELD="CLOSED_LIST">
	            <ITEMS DEFAULT="CONNECTIONTIMEOUT">
	                <ITEM NAME="CONNECTIONTIMEOUT" VALUE="ConnectionTimeout"/>
	                <ITEM NAME="MAXCONNECTIONS" VALUE="MaxConnections"/>
	                <ITEM NAME="MAXERRORRETRY" VALUE="MaxErrorRetry"/>
	                <ITEM NAME="PROTOCOL" VALUE="Protocol"/>
	                <ITEM NAME="PROXYDOMAIN" VALUE="ProxyDomain"/>
	                <ITEM NAME="PROXYHOST" VALUE="ProxyHost"/>
	                <ITEM NAME="PROXYPASSWORD" VALUE="ProxyPassword"/>
	                <ITEM NAME="PROXYPORT" VALUE="ProxyPort"/>
	                <ITEM NAME="PROXYUSERNAME" VALUE="ProxyUsername"/>
	                <ITEM NAME="PROXYWORKSTATION" VALUE="ProxyWorkstation"/>
	                <ITEM NAME="SOCKETTIMEOUT" VALUE="SocketTimeout"/>
	                <ITEM NAME="USERAGENT" VALUE="UserAgent"/>
	                <ITEM NAME="SOCKETRECEIVEBUFFERSIZEHINT" VALUE="SocketReceiveBufferSizeHints"/>
	                <ITEM NAME="SOCKETSENDBUFFERSIZEHINT" VALUE="SocketSendBufferSizeHints"/>
	            </ITEMS>
	        </ITEM>
	        <ITEM NAME="VALUE" />
	      </ITEMS>
	    </PARAMETER>
	    
	    <PARAMETER
      	  NAME="SET_STS_ENDPOINT"
      	  FIELD="CHECK"
      	  NUM_ROW="35"
      	  SHOW_IF="isShow[ASSUME_ROLE] AND (ASSUME_ROLE == 'true')"
    	>
    	</PARAMETER>
		<PARAMETER
      	  NAME="STS_ENDPOINT"
      	  FIELD="TEXT"
      	  NUM_ROW="35"
      	  REQUIRED="true"
      	  SHOW_IF="isShow[SET_STS_ENDPOINT] AND (SET_STS_ENDPOINT == 'true')"
    	>
      	  <DEFAULT>""</DEFAULT>
    	</PARAMETER>

    </ADVANCED_PARAMETERS>

   <CODEGENERATION>
    <IMPORTS>
       <IMPORT NAME="aws-java-sdk-1.10.52.jar" MODULE="aws-java-sdk-1.10.52.jar"
			MVN="mvn:org.talend.libraries/aws-java-sdk-1.10.52/6.0.0"
			REQUIRED_IF="USE_EXISTING_CONNECTION == 'false'" />
		<IMPORT NAME="jackson-core-2.5.3.jar" MODULE="jackson-core-2.5.3.jar"
			MVN="mvn:org.talend.libraries/jackson-core-2.5.3/6.0.0"
			UrlPath="platform:/plugin/org.talend.libraries.jackson/lib/jackson-core-2.5.3.jar"
			REQUIRED_IF="USE_EXISTING_CONNECTION == 'false'" />
		<IMPORT NAME="jackson-databind-2.5.3.jar" MODULE="jackson-databind-2.5.3.jar"
			MVN="mvn:org.talend.libraries/jackson-databind-2.5.3/6.0.0"
			UrlPath="platform:/plugin/org.talend.libraries.jackson/lib/jackson-databind-2.5.3.jar"
			REQUIRED_IF="USE_EXISTING_CONNECTION == 'false'" />
		<IMPORT NAME="jackson-annotations-2.5.3.jar" MODULE="jackson-annotations-2.5.3.jar"
			MVN="mvn:org.talend.libraries/jackson-annotations-2.5.3/6.0.0"
			UrlPath="platform:/plugin/org.talend.libraries.jackson/lib/jackson-annotations-2.5.3.jar"
			REQUIRED_IF="USE_EXISTING_CONNECTION == 'false'" />
		<IMPORT NAME="httpcore-4.3.3.jar" MODULE="httpcore-4.3.3.jar"
			MVN="mvn:org.talend.libraries/httpcore-4.3.3/6.0.0"
			UrlPath="platform:/plugin/org.talend.libraries.apache.http/lib/httpcore-4.3.3.jar"
			REQUIRED_IF="USE_EXISTING_CONNECTION == 'false'" />
		<IMPORT NAME="httpclient-4.3.6.jar" MODULE="httpclient-4.3.6.jar"
			MVN="mvn:org.talend.libraries/httpclient-4.3.6/6.0.0"
			UrlPath="platform:/plugin/org.talend.libraries.apache.http/lib/httpclient-4.3.6.jar"
			REQUIRED_IF="USE_EXISTING_CONNECTION == 'false'" />
		<IMPORT NAME="joda-time-2.8.1.jar" MODULE="joda-time-2.8.1.jar"
			MVN="mvn:org.talend.libraries/joda-time-2.8.1/6.0.0"
			REQUIRED_IF="USE_EXISTING_CONNECTION == 'false'" />
		<IMPORT NAME="commons-logging-1.1.3.jar" MODULE="commons-logging-1.1.3.jar"
			MVN="mvn:org.talend.libraries/commons-logging-1.1.3/6.0.0"
			UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-logging-1.1.3.jar"
			REQUIRED_IF="USE_EXISTING_CONNECTION == 'false'" />
		<IMPORT NAME="commons-codec-1.6.jar" MODULE="commons-codec-1.6.jar"
			MVN="mvn:org.talend.libraries/commons-codec-1.6/6.0.0"
			UrlPath="platform:/plugin/org.talend.libraries.apache.common/lib/commons-codec-1.6.jar"
			REQUIRED_IF="USE_EXISTING_CONNECTION == 'false'" />
    </IMPORTS>
   </CODEGENERATION>

   <RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>
