<COMPONENT>

<HEADER
  PLATEFORM="ALL"
  SERIAL=""
  VERSION="0.01"
  STATUS="ALPHA"
  COMPATIBILITY="ALL"
  AUTHOR="cahsohto"
  RELEASE_DATE="20080303A"
  STARTABLE="false"
  PARTITIONING="NONE"
  >
  <SIGNATURE></SIGNATURE>
</HEADER>

<FAMILIES>
  <FAMILY>Business_Intelligence/Charts</FAMILY>
</FAMILIES>

<DOCUMENTATION>
  <URL></URL>
</DOCUMENTATION>

<CONNECTORS>
  <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
  <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
  <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
  <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="0" />
  <CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="0"/>
  <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="0"/>
  <CONNECTOR CTYPE="RUN_IF" MAX_INPUT="0"/>
</CONNECTORS>

<PARAMETERS>
  <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1">
    <TABLE READONLY="false">
      <COLUMN NAME="series" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true"/>
      <COLUMN NAME="category" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true"/>
      <COLUMN NAME="value" TYPE="id_Integer" READONLY="true" CUSTOM="true"/>
    </TABLE>
  </PARAMETER>

  <PARAMETER NAME="GENERATED_IMAGE_PATH" FIELD="FILE" NUM_ROW="5" REQUIRED="true">
    <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.png"</DEFAULT>
  </PARAMETER>

  <PARAMETER NAME="CHART_TITLE" FIELD="STRING" NUM_ROW="10" REQUIRED="true">
    <DEFAULT>"Chart title"</DEFAULT>
  </PARAMETER>

  <PARAMETER NAME="INCLUDE_LEGEND" FIELD="CHECK" NUM_ROW="15" REQUIRED="true">
    <DEFAULT>true</DEFAULT>
  </PARAMETER>

  <PARAMETER NAME="3D_FORMAT" FIELD="CHECK" NUM_ROW="15" REQUIRED="true">
    <DEFAULT>true</DEFAULT>
  </PARAMETER>

  <PARAMETER NAME="IMAGE_WIDTH" FIELD="INTEGER" NUM_ROW="20" REQUIRED="true">
    <DEFAULT>500</DEFAULT>
  </PARAMETER>

  <PARAMETER NAME="IMAGE_HEIGHT" FIELD="INTEGER" NUM_ROW="20" REQUIRED="true">
    <DEFAULT>500</DEFAULT>
  </PARAMETER>

  <PARAMETER NAME="CATEGORY_AXIS" FIELD="String" NUM_ROW="25" REQUIRED="true">
    <DEFAULT>""</DEFAULT>
  </PARAMETER>

  <PARAMETER NAME="VALUE_AXIS" FIELD="String" NUM_ROW="25" REQUIRED="true">
    <DEFAULT>""</DEFAULT>
  </PARAMETER>

  <PARAMETER NAME="FORE_GROUND_ALPHA" FIELD="FLOAT" NUM_ROW="30" REQUIRED="true">
    <DEFAULT>0.8</DEFAULT>
  </PARAMETER>

  <PARAMETER NAME="PLOT_ORIENTATION" FIELD="CLOSED_LIST" NUM_ROW="35" REQUIRED="true">
    <ITEMS DEFAULT="VERTICAL">
      <ITEM NAME="VERTICAL" VALUE="org.jfree.chart.plot.PlotOrientation.VERTICAL" />
      <ITEM NAME="HORIZONTAL" VALUE="org.jfree.chart.plot.PlotOrientation.HORIZONTAL"/>
    </ITEMS>
  </PARAMETER>
</PARAMETERS>

<CODEGENERATION>
  <IMPORTS>
    <IMPORT NAME="JFreeChart" MODULE="jfreechart-1.0.3.jar" MVN="mvn:org.talend.libraries/jfreechart-1.0.3/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.jfreechart/lib/jfreechart-1.0.3.jar" REQUIRED="true"/>
    <IMPORT NAME="JCommon" MODULE="jcommon-1.0.6.jar" MVN="mvn:org.talend.libraries/jcommon-1.0.6/6.0.0"  REQUIRED="true"/>
  </IMPORTS>
</CODEGENERATION>

<RETURNS>
  <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
</RETURNS>

</COMPONENT>
