<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070323A"
    STARTABLE="false"
    DATA_AUTO_PROPAGATE="false"
	PARTITIONING="NONE"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/Teradata</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER NAME="PROPERTY_DATA" FIELD="PROPERTY_TYPE" SHOW="true" NUM_ROW="1" REPOSITORY_VALUE="DELIMITED" GROUP="DATA_FILE"/>

    <PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="2" REQUIRED="true" REPOSITORY_VALUE="FILE_PATH" GROUP="DATA_FILE">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.csv"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="APPEND" FIELD="CHECK" REQUIRED="true" NUM_ROW="3" REPOSITORY_VALUE="APPEND" GROUP="DATA_FILE">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PROPERTY_TPT"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:TERADATA"
    />
    <PARAMETER
      NAME="EXECUTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="12"
      REQUIRED="true"
    >
      <ITEMS DEFAULT="WINDOWS">
    		<ITEM NAME="WINDOWS" VALUE="Windows" />
    		<ITEM NAME="UNIX" VALUE="Unix" />
    	</ITEMS>
    </PARAMETER>

	<PARAMETER
      NAME="TDPID"
      FIELD="TDPID"
      NUM_ROW="13"
      REPOSITORY_VALUE="SERVER_NAME"
      REQUIRED="true"
      GROUP="CONNECTION"
    >
      <DEFAULT>"localtd"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="13"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      GROUP="CONNECTION"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>


    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="14"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
      GROUP="CONNECTION"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="14"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
      GROUP="CONNECTION"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="17"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

   <PARAMETER
    NAME="NOTE"
    FIELD="LABEL"
    NUM_ROW="17"
    >
    <DEFAULT>*Note:You must specify one or more columns as primary key for updating or deleting</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="18"
      REQUIRED="true"
      GROUP="CONSUMER"
    >
      <ITEMS DEFAULT="LOAD">
        <ITEM NAME="LOAD" VALUE="Load" />
        <ITEM NAME="INSERTER" VALUE="Inserter" />
        <ITEM NAME="UPDATE" VALUE="Update" />
        <ITEM NAME="STREAM" VALUE="Stream" />
      </ITEMS>
    </PARAMETER>

     <PARAMETER
      NAME="DML_TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="18"
      REQUIRED="true"
      GROUP="CONSUMER"
      SHOW_IF="(ACTION=='UPDATE') or (ACTION=='STREAM')"
    >
      <ITEMS DEFAULT="UPSERT">
        <ITEM NAME="INSERT" VALUE="Insert" />
        <ITEM NAME="UPDATE" VALUE="Update" />
        <ITEM NAME="UPSERT" VALUE="InsertOrUpdate" />
        <ITEM NAME="DELETE" VALUE="Delete" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="PRODUCER_TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="20"
      REQUIRED="true"
      GROUP="PRODUCER"
    >
      <ITEMS DEFAULT="DATA_CONNECTOR">
        <ITEM NAME="DATA_CONNECTOR" VALUE="DataConnector" />
        <!--
        <ITEM NAME="ODBC" VALUE="ODBC" />
        <ITEM NAME="FASTEXPORT_INMOD_ADAPTER" VALUE="FastExport INMOD" />
        <ITEM NAME="FASTLOAD_INMOD_ADAPTER" VALUE="FastLoad INMOD" />
        <ITEM NAME="MULTILOAD_INMOD_ADAPTER" VALUE="MultiLoad INMOD" />
        -->
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="1"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>Teradata</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="DBTABLE"
      NUM_ROW="22"
      REQUIRED="true"
    >
      <DEFAULT>"mytable"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCRIPT_PATH"
      FIELD="DIRECTORY"
      NUM_ROW="23"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/"</DEFAULT>
    </PARAMETER>

 	<PARAMETER
      NAME="ERROR_FILE"
      FIELD="FILE"
      NUM_ROW="26"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/error.log"</DEFAULT>
    </PARAMETER>
  </PARAMETERS>
  <ADVANCED_PARAMETERS>

	<PARAMETER NAME="ROWSEPARATOR" FIELD="TEXT" NUM_ROW="3" REPOSITORY_VALUE="ROW_SEPARATOR" GROUP="DATA_FILE">
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="FIELDSEPARATOR" FIELD="TEXT" NUM_ROW="3" REPOSITORY_VALUE="FIELD_SEPARATOR" GROUP="DATA_FILE">
      <DEFAULT>";"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="INCLUDEHEADER" FIELD="CHECK" NUM_ROW="4" GROUP="DATA_FILE">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="6" REQUIRED="true" REPOSITORY_VALUE="ENCODING" GROUP="DATA_FILE">
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCRIPT_PARAMETER"
      FIELD="CHECK"
      NUM_ROW="7"
      REQUIRED="true"
    >
      <DEFAULT>false</DEFAULT>
  </PARAMETER>

    <PARAMETER
      NAME="OPERATOR_LOAD"
      FIELD="TEXT"
      NUM_ROW="8"
      REPOSITORY_VALUE="OPERATOR_LOAD"
      REQUIRED="true"
      GROUP="SCRIPT_PARAMETERS"
      SHOW_IF="SCRIPT_PARAMETER=='true'"
    >
      <DEFAULT>"OPERATOR_LOAD"</DEFAULT>
    </PARAMETER>
  <PARAMETER
      NAME="OPERATOR_DATA_CONNECTOR"
      FIELD="TEXT"
      NUM_ROW="8"
      REPOSITORY_VALUE="OPERATOR_DATA_CONNECTOR"
      REQUIRED="true"
      GROUP="SCRIPT_PARAMETERS"
      SHOW_IF="SCRIPT_PARAMETER=='true'"
    >
      <DEFAULT>"OPERATOR_DATA_CONNECTOR"</DEFAULT>
    </PARAMETER>
  <PARAMETER
      NAME="JOB_NAME"
      FIELD="TEXT"
      NUM_ROW="9"
      REPOSITORY_VALUE="JOB_NAME"
      REQUIRED="true"
      GROUP="SCRIPT_PARAMETERS"
      SHOW_IF="SCRIPT_PARAMETER=='true'"
    >
      <DEFAULT>"JOB_NAME"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LAYOUT_NAME"
      FIELD="TEXT"
      NUM_ROW="9"
      REPOSITORY_VALUE="LAYOUT_NAME"
      REQUIRED="true"
      GROUP="SCRIPT_PARAMETERS"
      SHOW_IF="SCRIPT_PARAMETER=='true'"
    >
      <DEFAULT>"LAYOUT_NAME"</DEFAULT>
    </PARAMETER>

	<PARAMETER
	    NAME="SPECIFY_LOG_TABLE"
	    FIELD="CHECK"
	    NUM_ROW="10"
	    REQUIRED="true"
      SHOW_IF="ACTION != 'INSERTER'"
	>
	    <DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER
	    NAME="LOG_TABLE_TABLE"
	    FIELD="TEXT"
	    NUM_ROW="10"
	    REQUIRED="true"
		SHOW_IF="SPECIFY_LOG_TABLE=='true'"
	>
	    <DEFAULT>""</DEFAULT>
	</PARAMETER>
	<PARAMETER
	    NAME="RETURN_CODE_DIE"
	    FIELD="CHECK"
	    NUM_ROW="13"
	    REQUIRED="true"
	>
	    <DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER
	    NAME="RETURN_CODE"
	    FIELD="TEXT"
	    NUM_ROW="13"
	    REQUIRED="true"
		SHOW_IF="RETURN_CODE_DIE=='true'"
	>
	    <DEFAULT>8</DEFAULT>
	</PARAMETER>
	<PARAMETER
	    NAME="DEFINE_CHARSET"
	    FIELD="CHECK"
	    NUM_ROW="14"
	    REQUIRED="true"
	>
	    <DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER
	    NAME="CHARSET"
	    FIELD="TEXT"
	    NUM_ROW="14"
	    REQUIRED="true"
		SHOW_IF="DEFINE_CHARSET=='true'"
	>
   	    <DEFAULT>"ASCII"</DEFAULT>
 	</PARAMETER>

  <!-- TPT Consumer Operator Optional Attributes -->
  <PARAMETER NAME="APPLY_OPTIONAL_ATTRIBUTES" FIELD="CHECK" NUM_ROW="55" GROUP="CONSUMER_OPERATOR_OPTIONAL_ATTRIBUTES">
      <DEFAULT>false</DEFAULT>
  </PARAMETER>

  <PARAMETER NAME="NOTE_FOR_TPT_ATTRIS" FIELD="LABEL" NUM_ROW="60" GROUP="CONSUMER_OPERATOR_OPTIONAL_ATTRIBUTES"
             SHOW_IF="APPLY_OPTIONAL_ATTRIBUTES == 'true'">
  <DEFAULT>Note: Separate values with comma for array type.</DEFAULT>
  </PARAMETER>
  
  <!-- For LOAD -->
  <PARAMETER NAME="TPT_CONSUMER_OPERATOR_OPTIONAL_ATTRIBUTES" FIELD="TABLE"
  NUM_ROW="75" NB_LINES="10" GROUP="CONSUMER_OPERATOR_OPTIONAL_ATTRIBUTES"
  SHOW_IF="(APPLY_OPTIONAL_ATTRIBUTES == 'true') and (ACTION=='LOAD') ">
      <ITEMS >
        <ITEM NAME="OPTIONAL_ATTRIBUTES_NAME" FIELD="CLOSED_LIST">
        <ITEMS DEFAULT="ERRORLIMIT">
          <ITEM NAME="BUFFERSIZE" VALUE="BUFFERSIZE"/>
          <ITEM NAME="ERRORLIMIT" VALUE="ERRORLIMIT"/>
          <ITEM NAME="MAXSESSIONS" VALUE="MAXSESSIONS"/>
          <ITEM NAME="MINSESSIONS" VALUE="MINSESSIONS"/>
          <ITEM NAME="TENACITYHOURS" VALUE="TENACITYHOURS"/>
          <ITEM NAME="TENACITYSLEEP" VALUE="TENACITYSLEEP"/>
          <ITEM NAME="ACCOUNTID" VALUE="ACCOUNTID"/>
          <ITEM NAME="DATAENCRYPTION" VALUE="DATAENCRYPTION"/>
          <ITEM NAME="DATEFORM" VALUE="DATEFORM"/>
          <ITEM NAME="ERRORTABLE1" VALUE="ERRORTABLE1"/>
          <ITEM NAME="ERRORTABLE2" VALUE="ERRORTABLE2"/>
          <ITEM NAME="LOGONMECH" VALUE="LOGONMECH"/>
          <ITEM NAME="LOGONMECHDATA" VALUE="LOGONMECHDATA"/>
          <ITEM NAME="NOTIFYEXIT" VALUE="NOTIFYEXIT"/>
          <ITEM NAME="NOTIFYEXITISDLL" VALUE="NOTIFYEXITISDLL"/>
          <ITEM NAME="NOTIFYLEVEL" VALUE="NOTIFYLEVEL"/>
          <ITEM NAME="NOTIFYMETHOD" VALUE="NOTIFYMETHOD"/>
          <ITEM NAME="LOGSQL" VALUE="LOGSQL"/>
          <ITEM NAME="NOTIFYSTRING" VALUE="NOTIFYSTRING"/>
          <ITEM NAME="PAUSEACQ" VALUE="PAUSEACQ"/>
          <ITEM NAME="PRIVATELOGNAME" VALUE="PRIVATELOGNAME"/>
          <ITEM NAME="QUERYBANDSESSINFO" VALUE="QUERYBANDSESSINFO"/>
          <ITEM NAME="WILDCARDINSERT" VALUE="WILDCARDINSERT"/>
          <ITEM NAME="WORKINGDATABASE" VALUE="WORKINGDATABASE"/>
          <ITEM NAME="TRACELEVEL" VALUE="TRACELEVEL"/>
        </ITEMS>
        </ITEM>
        <ITEM NAME="OPTIONAL_ATTRIBUTES_VALUE" FIELD="TEXT" />
      </ITEMS>
  </PARAMETER>

  <!-- For INSERTER -->
   <PARAMETER NAME="TPT_COOA_FOR_INSERTER" FIELD="TABLE"
    NUM_ROW="75" NB_LINES="10" GROUP="CONSUMER_OPERATOR_OPTIONAL_ATTRIBUTES"
    SHOW_IF="(APPLY_OPTIONAL_ATTRIBUTES == 'true') and (ACTION=='INSERTER') ">
      <ITEMS >
        <ITEM NAME="OPTIONAL_ATTRIBUTES_NAME" FIELD="CLOSED_LIST">
        <ITEMS DEFAULT="ACCOUNTID">
          <ITEM NAME="ACCOUNTID" VALUE="ACCOUNTID"/>
          <ITEM NAME="DATAENCRYPTION" VALUE="DATAENCRYPTION"/>
          <ITEM NAME="DATEFORM" VALUE="DATEFORM"/>
          <ITEM NAME="LOGONMECH" VALUE="LOGONMECH"/>
          <ITEM NAME="LOGONMECHDATA" VALUE="LOGONMECHDATA"/>
          <ITEM NAME="LOGSQL" VALUE="LOGSQL"/>
          <ITEM NAME="PRIVATELOGNAME" VALUE="PRIVATELOGNAME"/>
          <ITEM NAME="QUERYBANDSESSINFO" VALUE="QUERYBANDSESSINFO"/>
          <ITEM NAME="REPLICATIONOVERRIDE" VALUE="REPLICATIONOVERRIDE"/>
          <ITEM NAME="TRACELEVEL" VALUE="TRACELEVEL"/>
          <ITEM NAME="WORKINGDATABASE" VALUE="WORKINGDATABASE"/>
        </ITEMS>
        </ITEM>
        <ITEM NAME="OPTIONAL_ATTRIBUTES_VALUE" FIELD="TEXT" />
      </ITEMS>
  </PARAMETER>

  <!-- For UPDATE -->
   <PARAMETER NAME="TPT_COOA_FOR_UPDATE" FIELD="TABLE"
    NUM_ROW="75" NB_LINES="10" GROUP="CONSUMER_OPERATOR_OPTIONAL_ATTRIBUTES"
    SHOW_IF="(APPLY_OPTIONAL_ATTRIBUTES == 'true') and (ACTION=='UPDATE') ">
      <ITEMS >
        <ITEM NAME="OPTIONAL_ATTRIBUTES_NAME" FIELD="CLOSED_LIST">
        <ITEMS DEFAULT="ERRORLIMIT">
         <ITEM NAME="PRIVATELOGNAME" VALUE="PRIVATELOGNAME"/>
          <ITEM NAME="BUFFERSIZE" VALUE="BUFFERSIZE"/>
          <ITEM NAME="ERRORLIMIT" VALUE="ERRORLIMIT"/>
          <ITEM NAME="MAXSESSIONS" VALUE="MAXSESSIONS"/>
          <ITEM NAME="MINSESSIONS" VALUE="MINSESSIONS"/>
          <ITEM NAME="TENACITYHOURS" VALUE="TENACITYHOURS"/>
          <ITEM NAME="TENACITYSLEEP" VALUE="TENACITYSLEEP"/>
          <ITEM NAME="ACCOUNTID" VALUE="ACCOUNTID"/>
          <ITEM NAME="AMPCHECK" VALUE="AMPCHECK"/>
          <ITEM NAME="DATAENCRYPTION" VALUE="DATAENCRYPTION"/>
          <ITEM NAME="DATEFORM" VALUE="DATEFORM"/>
          <ITEM NAME="DELETETASK" VALUE="DELETETASK"/>
          <ITEM NAME="DROPERRORTABLE" VALUE="DROPERRORTABLE"/>
          <ITEM NAME="DROPLOGTABLE" VALUE="DROPLOGTABLE"/>
          <ITEM NAME="DROPWORKTABLE" VALUE="DROPWORKTABLE"/>
          <ITEM NAME="ERRORTABLE1" VALUE="ERRORTABLE1"/>
          <ITEM NAME="ERRORTABLE2" VALUE="ERRORTABLE2"/>
          <ITEM NAME="LOGONMECH" VALUE="LOGONMECH"/>
          <ITEM NAME="LOGONMECHDATA" VALUE="LOGONMECHDATA"/>
          <ITEM NAME="LOGSQL" VALUE="LOGSQL"/>
          <ITEM NAME="NOTIFYEXIT" VALUE="NOTIFYEXIT"/>
          <ITEM NAME="NOTIFYEXITISDLL" VALUE="NOTIFYEXITISDLL"/>
          <ITEM NAME="NOTIFYLEVEL" VALUE="NOTIFYLEVEL"/>
          <ITEM NAME="NOTIFYMETHOD" VALUE="NOTIFYMETHOD"/>
          <ITEM NAME="NOTIFYSTRING" VALUE="NOTIFYSTRING"/>
          <ITEM NAME="PAUSEACQ" VALUE="PAUSEACQ"/>
          <ITEM NAME="QUERYBANDSESSINFO" VALUE="QUERYBANDSESSINFO"/>
          <ITEM NAME="QUEUEERRORTABLE" VALUE="QUEUEERRORTABLE"/>
          <ITEM NAME="WORKINGDATABASE" VALUE="WORKINGDATABASE"/>
          <ITEM NAME="WORKTABLE" VALUE="WORKTABLE"/>
          <ITEM NAME="TRACELEVEL" VALUE="TRACELEVEL"/>
        </ITEMS>
        </ITEM>
        <ITEM NAME="OPTIONAL_ATTRIBUTES_VALUE" FIELD="TEXT" />
      </ITEMS>
  </PARAMETER>
  
  <!-- For Stream -->
  <PARAMETER NAME="TPT_COOA_FOR_STREAM" FIELD="TABLE"
    NUM_ROW="75" NB_LINES="10" GROUP="CONSUMER_OPERATOR_OPTIONAL_ATTRIBUTES"
    SHOW_IF="(APPLY_OPTIONAL_ATTRIBUTES == 'true') and (ACTION=='STREAM') ">
      <ITEMS >
        <ITEM NAME="OPTIONAL_ATTRIBUTES_NAME" FIELD="CLOSED_LIST">
        <ITEMS DEFAULT="ERRORLIMIT">
         <ITEM NAME="PRIVATELOGNAME" VALUE="PRIVATELOGNAME"/>
          <ITEM NAME="BUFFERS" VALUE="BUFFERS"/>
          <ITEM NAME="ERRORLIMIT" VALUE="ERRORLIMIT"/>
          <ITEM NAME="MAXSESSIONS" VALUE="MAXSESSIONS"/>
          <ITEM NAME="MINSESSIONS" VALUE="MINSESSIONS"/>
          <ITEM NAME="PACK" VALUE="PACK"/>
          <ITEM NAME="RATE" VALUE="RATE"/>
          <ITEM NAME="PERIODICITY" VALUE="PERIODICITY"/>
          <ITEM NAME="TENACITYHOURS" VALUE="TENACITYHOURS"/>
          <ITEM NAME="TENACITYSLEEP" VALUE="TENACITYSLEEP"/>
          <ITEM NAME="ACCOUNTID" VALUE="ACCOUNTID"/>
          <ITEM NAME="APPENDERRORTABLE" VALUE="APPENDERRORTABLE"/>
          <ITEM NAME="ARRAYSUPPORT" VALUE="ARRAYSUPPORT"/>
          <ITEM NAME="DATAENCRYPTION" VALUE="DATAENCRYPTION"/>
          <ITEM NAME="DATEFORM" VALUE="DATEFORM"/>
          <ITEM NAME="DROPERRORTABLE" VALUE="DROPERRORTABLE"/>
          <ITEM NAME="DROPMACRO" VALUE="DROPMACRO"/>
          <ITEM NAME="ERRORTABLE" VALUE="ERRORTABLE"/>
          <ITEM NAME="LOGONMECH" VALUE="LOGONMECH"/>
          <ITEM NAME="LOGONMECHDATA" VALUE="LOGONMECHDATA"/>
          <ITEM NAME="MACRODATABASE" VALUE="MACRODATABASE"/>
          <ITEM NAME="OPERATORCOMMANDID" VALUE="OPERATORCOMMANDID"/>
          <ITEM NAME="NOTIFYEXIT" VALUE="NOTIFYEXIT"/>
          <ITEM NAME="NOTIFYEXITISDLL" VALUE="NOTIFYEXITISDLL"/>
          <ITEM NAME="NOTIFYLEVEL" VALUE="NOTIFYLEVEL"/>
          <ITEM NAME="NOTIFYMETHOD" VALUE="NOTIFYMETHOD"/>
          <ITEM NAME="LOGSQL" VALUE="LOGSQL"/>
          <ITEM NAME="NOTIFYSTRING" VALUE="NOTIFYSTRING"/>
          <ITEM NAME="PACKMAXIMUM" VALUE="PACKMAXIMUM"/>
          <ITEM NAME="QUERYBANDSESSINFO" VALUE="QUERYBANDSESSINFO"/>
          <ITEM NAME="QUEUEERRORTABLE" VALUE="QUEUEERRORTABLE"/>
          <ITEM NAME="REPLICATIONOVERRRIDE" VALUE="REPLICATIONOVERRRIDE"/>
          <ITEM NAME="ROBUST" VALUE="ROBUST"/>
          <ITEM NAME="WORKINGDATABASE" VALUE="WORKINGDATABASE"/>
          <ITEM NAME="TRACELEVEL" VALUE="TRACELEVEL"/>
        </ITEMS>
        </ITEM>
        <ITEM NAME="OPTIONAL_ATTRIBUTES_VALUE" FIELD="TEXT"/>
      </ITEMS>
  </PARAMETER>

  </ADVANCED_PARAMETERS>

<CODEGENERATION>
    <TEMPLATES INPUT="tTPTOutput" OUTPUT="tTPTInput">

      <TEMPLATE NAME="tTPTOutput" COMPONENT="tTeradataTPTOutput">
        <LINK_TO NAME="tTPTInput" CTYPE="ROWS_END"/>
      </TEMPLATE>

      <TEMPLATE NAME="tTPTInput" COMPONENT="tTeradataTPTExec"/>

      <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tTPTOutput.FILENAME" />
      <TEMPLATE_PARAM SOURCE="self.APPEND" TARGET="tTPTOutput.APPEND" />
      <TEMPLATE_PARAM SOURCE="self.ROWSEPARATOR" TARGET="tTPTOutput.ROWSEPARATOR" />
	  <TEMPLATE_PARAM SOURCE="self.FIELDSEPARATOR" TARGET="tTPTOutput.FIELDSEPARATOR" />
      <TEMPLATE_PARAM SOURCE="self.INCLUDEHEADER" TARGET="tTPTOutput.INCLUDEHEADER" />
      <TEMPLATE_PARAM SOURCE="self.ENCODING" TARGET="tTPTOutput.ENCODING" />

      <TEMPLATE_PARAM SOURCE="self.TYPE" TARGET="tTPTInput.TYPE" />
	  <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tTPTInput.LOAD_FILE" />
      <TEMPLATE_PARAM SOURCE="self.ACTION" TARGET="tTPTInput.ACTION" />
	  <TEMPLATE_PARAM SOURCE="self.EXECUTION" TARGET="tTPTInput.EXECUTION" />
	  <TEMPLATE_PARAM SOURCE="self.TDPID" TARGET="tTPTInput.TDPID" />
      <TEMPLATE_PARAM SOURCE="self.DB_NAME" TARGET="tTPTInput.DB_NAME" />
	  <TEMPLATE_PARAM SOURCE="self.OPERATOR_LOAD" TARGET="tTPTInput.OPERATOR_LOAD" />
	  <TEMPLATE_PARAM SOURCE="self.OPERATOR_DATA_CONNECTOR" TARGET="tTPTInput.OPERATOR_DATA_CONNECTOR" />
	  <TEMPLATE_PARAM SOURCE="self.JOB_NAME" TARGET="tTPTInput.JOB_NAME" />
	  <TEMPLATE_PARAM SOURCE="self.DBNAME" TARGET="tTPTInput.DBNAME" />
	  <TEMPLATE_PARAM SOURCE="self.LAYOUT_NAME" TARGET="tTPTInput.LAYOUT_NAME" />
	  <TEMPLATE_PARAM SOURCE="self.USER" TARGET="tTPTInput.USER" />
	  <TEMPLATE_PARAM SOURCE="self.PASS" TARGET="tTPTInput.PASS" />
	  <TEMPLATE_PARAM SOURCE="self.TABLE" TARGET="tTPTInput.TABLE" />
	  <TEMPLATE_PARAM SOURCE="self.SCRIPT_PATH" TARGET="tTPTInput.SCRIPT_PATH" />
	  <TEMPLATE_PARAM SOURCE="self.ERROR_FILE" TARGET="tTPTInput.ERROR_FILE" />
	  <TEMPLATE_PARAM SOURCE="self.SPECIFY_LOG_TABLE" TARGET="tTPTInput.SPECIFY_LOG_TABLE" />
	  <TEMPLATE_PARAM SOURCE="self.LOG_TABLE_TABLE" TARGET="tTPTInput.LOG_TABLE_TABLE" />
	  <TEMPLATE_PARAM SOURCE="self.RETURN_CODE_DIE" TARGET="tTPTInput.RETURN_CODE_DIE" />
	  <TEMPLATE_PARAM SOURCE="self.RETURN_CODE" TARGET="tTPTInput.RETURN_CODE" />
	  <TEMPLATE_PARAM SOURCE="self.DEFINE_CHARSET" TARGET="tTPTInput.DEFINE_CHARSET" />
	  <TEMPLATE_PARAM SOURCE="self.CHARSET" TARGET="tTPTInput.CHARSET" />
	  <TEMPLATE_PARAM SOURCE="self.FIELDSEPARATOR" TARGET="tTPTInput.FIELD_SEPARATOR" />

	  <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="tTPTInput.SCHEMA" />
	  <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="tTPTOutput.SCHEMA" />

	  <TEMPLATE_PARAM SOURCE="self.PROPERTY_TPT" TARGET="tTPTInput.PROPERTY" />
	  <TEMPLATE_PARAM SOURCE="self.PROPERTY_DATA" TARGET="tTPTOutput.PROPERTY" />

    <TEMPLATE_PARAM SOURCE="self.SCRIPT_PARAMETER" TARGET="tTPTInput.SCRIPT_PARAMETER" />
    <TEMPLATE_PARAM SOURCE="self.DML_TYPE" TARGET="tTPTInput.DML_TYPE" />

    <TEMPLATE_PARAM SOURCE="self.APPLY_OPTIONAL_ATTRIBUTES" TARGET="tTPTInput.APPLY_OPTIONAL_ATTRIBUTES" />
    <TEMPLATE_PARAM SOURCE="self.TPT_CONSUMER_OPERATOR_OPTIONAL_ATTRIBUTES" TARGET="tTPTInput.TPT_CONSUMER_OPERATOR_OPTIONAL_ATTRIBUTES" />
    <TEMPLATE_PARAM SOURCE="self.TPT_COOA_FOR_INSERTER" TARGET="tTPTInput.TPT_COOA_FOR_INSERTER" />
    <TEMPLATE_PARAM SOURCE="self.TPT_COOA_FOR_UPDATE" TARGET="tTPTInput.TPT_COOA_FOR_UPDATE" />
    <TEMPLATE_PARAM SOURCE="self.TPT_COOA_FOR_STREAM" TARGET="tTPTInput.TPT_COOA_FOR_STREAM" />

    </TEMPLATES>
  </CODEGENERATION>

  <RETURNS>
	<RETURN NAME="EXIT_VALUE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
