    PARALLELIZE="true"
	PARTITIONING="AUTO"
    LOG4J_ENABLED="true"
	>
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
	<FAMILY>Business_Intelligence/OLAP_Cube/Palo</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

<CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"  MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>




  <PARAMETERS>

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="2"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      REQUIRED="true" FILTER="tPaloConnection"
      NUM_ROW="3"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />

	<PARAMETER
		NAME="SERVER"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="5"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"localhost"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="SERVERPORT"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="6"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"7777"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="USERNAME"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="7"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
  	>
	  <DEFAULT>"admin"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="PASS"
		FIELD="PASSWORD"
		REQUIRED="true"
		NUM_ROW="8"
		GROUP="CONNECTION"
		SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
	>
	  <DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="DATABASE"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="10"
	>
	  <DEFAULT>"Database"</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="CUBE"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="11"
	>
	  <DEFAULT>"Cube"</DEFAULT>
	</PARAMETER>

	<PARAMETER
      	NAME="CUBE_TYPE"
	      FIELD="CLOSED_LIST"
      	NUM_ROW="11"
   	>
	<ITEMS DEFAULT="NORMAL">
        <ITEM NAME="NORMAL" VALUE="NORMAL" />
        <ITEM NAME="USER_INFO" VALUE="USER_INFO"/>
		<ITEM NAME="ATTRIBUT" VALUE="ATTRIBUT"/>
		<ITEM NAME="SYSTEM" VALUE="SYSTEM"/>
      </ITEMS>
    </PARAMETER>

	<PARAMETER
		NAME="COMMITSIZE"
		FIELD="TEXT"
		REQUIRED="true"
		NUM_ROW="11"
	>
	  <DEFAULT>1000</DEFAULT>
	</PARAMETER>


    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="30"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER
		NAME="MEASURE_COLUMN"
      	FIELD="PREV_COLUMN_LIST"
      	REQUIRED="true"
      	NUM_ROW="31"
    	>
    	</PARAMETER>

	<PARAMETER NAME="SPLASH_MODE" FIELD="CLOSED_LIST" NUM_ROW="35">
			<ITEMS DEFAULT="PALO_SPLASH_DEFAULT">
				<ITEM NAME="PALO_SPLASH_DISABLE" VALUE="PALO_SPLASH_DISABLE" />
				<ITEM NAME="PALO_SPLASH_DEFAULT" VALUE="PALO_SPLASH_DEFAULT" />
				<ITEM NAME="PALO_SPLASH_BASE_SET" VALUE="PALO_SPLASH_BASE_SET" />
				<ITEM NAME="PALO_SPLASH_BASE_ADD" VALUE="PALO_SPLASH_BASE_ADD" />
			</ITEMS>
		</PARAMETER>

	<PARAMETER
		NAME="ADDVALUES"
      	FIELD="CHECK"
      	NUM_ROW="35"
    	>
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER
		NAME="EVENTPROCESSOR"
      	FIELD="CHECK"
      	NUM_ROW="35"
    	>
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>

    <PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="101"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>


  </PARAMETERS>

  <ADVANCED_PARAMETERS>
	<PARAMETER NAME="DEPLOY_PALO_LIBS" FIELD="CHECK" NUM_ROW="200" SHOW="false">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

  </ADVANCED_PARAMETERS>
