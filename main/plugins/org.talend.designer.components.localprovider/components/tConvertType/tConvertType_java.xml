<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    HAS_CONDITIONAL_OUTPUTS="true"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
	PARTITIONING="AUTO"
    >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Processing</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MIN_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR NAME="REJECT" NOT_SHOW_IF="(DIEONERROR == 'true')" CTYPE="FLOW" MAX_INPUT="0"
			MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="IN_SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1">
      <DEFAULT/>
    </PARAMETER>
    
    <PARAMETER NAME="AUTOCAST" FIELD="CHECK" NUM_ROW="3">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>   
    
    <PARAMETER NAME="MANUALTABLE" FIELD="TABLE" NUM_ROW="5" NB_LINES="5" SHOW_IF=" AUTOCAST == 'false'">
      <ITEMS>
      	<ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
        <ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER NAME="SCHEMA_REJECT" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="7" CONTEXT="REJECT" SHOW="true">
      <TABLE READONLY="true">
        <COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true" />
        <COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true" />
      </TABLE>
    </PARAMETER>
    
    <PARAMETER NAME="EMPTYTONULL" FIELD="CHECK" NUM_ROW="8">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    		
	<PARAMETER NAME="DIEONERROR" FIELD="CHECK" NUM_ROW="10">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS/>
  
  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
