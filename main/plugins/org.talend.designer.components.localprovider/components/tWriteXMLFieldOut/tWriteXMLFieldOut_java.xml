<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="false" TECHNICAL="true"
	    SCHEMA_AUTO_PROPAGATE="false"
	    DATA_AUTO_PROPAGATE="false"
		PARTITIONING="NONE"
		LOG4J_ENABLED="true"
	>
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>XML</FAMILY>
  </FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="ROOT" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="10" NB_LINES="3" SHOW="false">
			<ITEMS>
				<ITEM NAME="PATH" FIELD="TEXT" />
				<ITEM NAME="COLUMN" FIELD="TEXT" />
				<ITEM NAME="VALUE" FIELD="TEXT" />
				<ITEM NAME="ATTRIBUTE" FIELD="TEXT" />
				<ITEM NAME="ORDER" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="GROUP" FIELD="TABLE" REQUIRED="false"
			NUM_ROW="20" NB_LINES="5" SHOW="false">
			<ITEMS>
				<ITEM NAME="PATH" FIELD="TEXT" />
				<ITEM NAME="COLUMN" FIELD="TEXT" />
				<ITEM NAME="VALUE" FIELD="TEXT" />
				<ITEM NAME="ATTRIBUTE" FIELD="TEXT" />
				<ITEM NAME="ORDER" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="LOOP" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="30" SHOW="false">
			<ITEMS>
				<ITEM NAME="PATH" FIELD="TEXT" />
				<ITEM NAME="COLUMN" FIELD="TEXT" />
				<ITEM NAME="VALUE" FIELD="TEXT" />
				<ITEM NAME="ATTRIBUTE" FIELD="TEXT" />
				<ITEM NAME="ORDER" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="50">
			<DEFAULT />
		</PARAMETER>

	    <PARAMETER
	      NAME="GROUPBYS"
	      FIELD="TABLE"
	      NUM_ROW="60"
	      NB_LINES="3"
	    >
	      <ITEMS>
	        <ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
	      </ITEMS>
	    </PARAMETER>

	    <PARAMETER NAME="JSONFIELD" FIELD="COLUMN_LIST" REQUIRED="true"
			NUM_ROW="9999" SHOW='false'>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="REMOVE_HEADER" FIELD="CHECK" NUM_ROW="5">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CREATE" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="10">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CREATE_EMPTY_ELEMENT" FIELD="CHECK"
			REQUIRED="false" NUM_ROW="11">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="EXPAND_EMPTY_ELM" FIELD="CHECK" NUM_ROW="11"
			SHOW_IF="CREATE_EMPTY_ELEMENT=='true'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="OUTPUT_AS_XSD" FIELD="CHECK"
			REQUIRED="false" NUM_ROW="15">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="XSD_FILE" FIELD="FILE"
			REQUIRED="false" NUM_ROW="16" SHOW_IF="OUTPUT_AS_XSD == 'true')">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/file.xsd"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ADVANCED_SEPARATOR" FIELD="CHECK" REQUIRED="true" NUM_ROW="31" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="THOUSANDS_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="32"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')">
			<DEFAULT>","</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DECIMAL_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="32"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')">
			<DEFAULT>"."</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="COMPACT_FORMAT" FIELD="CHECK" REQUIRED="true" NUM_ROW="35">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="GENERATION_MODE" FIELD="CLOSED_LIST" NUM_ROW="40">
	      <ITEMS DEFAULT="DOM4J">
	        <ITEM NAME="DOM4J" VALUE="Dom4j" />
	        <ITEM NAME="NULL" VALUE="Null"/>
	      </ITEMS>
	    </PARAMETER>

		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="50"
			REQUIRED="true">
			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="STORE_FLOW" FIELD="CHECK"
			REQUIRED="false" NUM_ROW="100" SHOW='false'>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

        <PARAMETER NAME="DESTINATION" FIELD="TEXT" RAW="true" NUM_ROW="20"
            REQUIRED="true">
            <DEFAULT>tWriteXMLFieldOut_1</DEFAULT>
        </PARAMETER>

	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="Java_DOM4J1.6" MODULE="dom4j-1.6.1.jar" MVN="mvn:org.talend.libraries/dom4j-1.6.1/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.dom4j-jaxen/lib/dom4j-1.6.1.jar"
				REQUIRED="true" BundleID="" />
			<IMPORT NAME="Java_JAXEN1.1" MODULE="jaxen-1.1.1.jar" MVN="mvn:org.talend.libraries/jaxen-1.1.1/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.dom4j-jaxen/lib/jaxen-1.1.1.jar"
				REQUIRED="true" BundleID="" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
		<RETURN NAME="XML_FIELD" TYPE="id_String" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>
